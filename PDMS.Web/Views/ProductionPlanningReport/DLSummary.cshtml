<section class="content portal-content">
    <!--次標題 與 搜尋-->
    <div class="row">
        <!--次標題與Search keyword-->
        <div class="col-md-12 col-lg-9">
            <h4 class="margin-td-5" id="js_search_keywords"></h4>
        </div><!-- /col-次標題與Search keyword -->
        <!--col-右方搜尋與功能按鈕列-->
        <div class="col-md-12 search-field col-lg-3">
            <a class="btn btn-primary btn-sm" data-toggle="modal" data-target="#js_search_modal" id="js_btn_search"><i class="fa fa-search"></i> @T("QA.Inquire")</a>
            @*<a id="btn_export" class="btn btn-primary btn-sm" role="button">
                <i class="glyphicon glyphicon-save"></i> @T("Common.Export")
            </a>*@
        </div><!-- /col-右方搜尋與功能按鈕列-->
    </div><!--/次標題 與 搜尋-->
    <div class="row">
        <div class="col-xs-12" id="js_criteria_alert_cnt">
        </div>
    </div>
    <div>
        <ul id="myTab"></ul>
        <div id="myTabContent" class="tab-content">
        </div>
    </div>
    <!--內容 表格列-->

    <div id="div_table" class="row">
        <!--表格-->
        <div class="col-md-12 table-container">
            @T("QA.Mainscheduleinput")
            <table class="table table-striped table-hover table-condensed nowrap" id="js_ppcheckdata_datatable">
                <thead>
                    <tr>
                        <th>@T("QA.Project")</th>
                        <th>@T("QA.Mondays")</th>
                        <th>@T("QA.Tuesdays")</th>
                        <th>@T("QA.Wednesdays")</th>
                        <th>@T("QA.Thursdays")</th>
                        <th>@T("QA.Fridays")</th>
                        <th>@T("QA.Saturdays")</th>
                        <th>@T("QA.Sundays")</th>
                    </tr>
                </thead>
            </table>
            @*周报*@
            <div>
                <ul id="myTab1"></ul>
                <div id="myTabContent1" class="tab-content">
                </div>
            </div>
            <table class="table table-striped table-hover table-condensed nowrap" id="js_ppcheckdata_week">
                <thead>
                    <tr id="trfun">
                        
                    </tr>
                </thead>

            </table>
            <table class="table table-striped table-hover table-condensed nowrap" id="js_ppcheckdata_Project">
                <thead>
                    <tr id="trPro">
                    </tr>
                </thead>

            </table>

            <table class="table table-striped table-hover table-condensed nowrap" id="js_ppcheckdata_MP">
                <thead>
                </thead>

            </table>
            <table class="table table-striped table-hover table-condensed nowrap" id="js_ppcheckdata_NPI">
                <thead>
                </thead>

            </table>

            <div id="page" class="row"></div>
        </div><!--/表格-->
    </div><!-- / 內容 表格列 -->
    @*<hr class="hr-custom">*@
</section><!-- /.content -->

