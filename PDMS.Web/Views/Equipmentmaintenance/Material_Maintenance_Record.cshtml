                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Notes">备注</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Notes" name="Notes" data-rule-maxlength="100">
                                </div>
                            </div>

                            <input type="hidden" name="Material_Maintenance_Record_UID" />
                            <div class="col-xs-12"><ul class="list-group validate-error"></ul></div>
                        </div>
                    </div>
                }

