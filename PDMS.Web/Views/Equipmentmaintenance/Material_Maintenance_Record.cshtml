                                <label class="col-sm-5 control-label" for="s_input_Vendor">维修厂商</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm required" id="s_input_Vendor" name="Vendor" data-rule-maxlength="100">
                                </div>
                            </div>
