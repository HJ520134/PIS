                                <label class="col-sm-5 control-label" for="s_input_s_input_Delivery_Date">送修日期</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm  date required" id="s_input_Delivery_Date" name="Delivery_Date" placeholder="Delivery_Date">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Expected_Return_Date">預計完成日期</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm date required" id="s_input_Expected_Return_Date" name="Expected_Return_Date" placeholder="Expected_Return_Date">
                                </div>
                            </div>
