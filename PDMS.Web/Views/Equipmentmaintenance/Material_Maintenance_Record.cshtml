                                <div class="col-sm-7">
                                    <select class="selectpicker form-control input-sm"  id="js_select_Is_Warranty" name="Is_Warranty">                                      
                                            <option value=true selected>是</option>
                                            <option value=false>否</option>
                                    </select>
                                </div>
                            </div>  
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Abnormal">异常判定</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm required" id="s_input_Abnormal" name="Abnormal" data-rule-maxlength="100">
                                </div>
                            </div>
                                                      
