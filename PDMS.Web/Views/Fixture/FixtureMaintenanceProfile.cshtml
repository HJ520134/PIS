        //OP类型或功能厂变更,取得治具LIST-----------START
        function SetFixtureNoByFunplant(BG_Organization_UID, FunPlant_Organization_UID, Fixture_NO) {
            $('#js_select_Fixture_NO').html('<option value=0></option>');
            var url = FixtureMaintenanceProfileUrl.urls.getFixtureNoByFunPlant;
            if (BG_Organization_UID != 0) {
                var arr = new Array();
                $.post(url, { BG_Organization_UID: BG_Organization_UID, FunPlant_Organization_UID: FunPlant_Organization_UID == null ? 0 : FunPlant_Organization_UID }, function (data) {
                    //for (var i = 0; i < data.length; i++) {
                    //    $('#js_select_Fixture_NO').append('<option value="' + data[i] + '">' + data[i] + '</option>');
                    //}

                    for (var i = 0; i < data.length; i++) {
                        var isRepeated = false;
                        $.each(arr, function (index, item) {
                            if (item == data[i]) {
                                isRepeated = true;
                            }
                        });
                        if (!isRepeated) {
                            arr.push(data[i]);
                        }
                    }
                    $.each(arr, function (index, item) {
                        $('#js_select_Fixture_NO').append('<option value="' + item + '">' + item + '</option>');
      
                    });
                   // $('#js_select_Fixture_NO').selectpicker('refresh');
                });
            }
            $('#js_select_Fixture_NO').selectpicker('refresh');
            $('#js_select_Fixture_NO').selectpicker('val', Fixture_NO);
            $("#js_select_Fixture_NO").trigger("liszt:updated");
        }
