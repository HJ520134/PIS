                                            @if (Model.funplants != null)
                                            {
                                                <option value=0></option>
                                                foreach (var item in Model.funplants)
                                                {
                                                    <option value=@item.System_FunPlant_UID>@item.FunPlant</option>
                                                }
                                            }
                                        </select>
                                    </div>
                                </div>
                            </div>

