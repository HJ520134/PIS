                                        <select class="selectpicker form-control input-sm" id="js_select_Cycle_ID" name="Maintenance_Plan_UID"  data-live-search="true">
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6 ">
