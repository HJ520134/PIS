                                        <select class="form-control input-sm" id="js_select_Maintenance_Type" name="Maintenance_Type">
                                            <option value="D">日常保养</option>
                                            <option value="P">周期保养</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label no-padding-hr" for="js_select_Cycle_ID">周期类别代码</label>
