@section ViewScripts{
<link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
<script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
    <script type="text/javascript">
    $('#btn_import_fl').on('click', function () {
        $('#js_importExcel_modal').modal('show');
    });

    $('.needint').keydown(function (event) {
        var value = event.key;
        if (value.length > 1)
            return true;
        if (!((/^(\+|-)?\d+$/.test(value) && value >= 0)))
            return false;
    })

    $('#js_btn_clear').click(function () {
        $('#js_select_plant_search').val(0);
        $('#js_select_op_types_search').val(0);
        $('#js_select_funplant_search').val(0);
        $('#js_select_Maintenance_Type_search').val('');
        $('#js_select_Cycle_ID_search').val(0);
        PDMS.Utility.Criteria.Clear();
    });

    $('#btn_clear_Update').on('click', function () {
        $('#js_importExcel_modal').modal('hide');
    });


    //#endregion
    $(function () {
        var FixtureMaintenanceProfileUrl = (function () {
            var urls = {
                //画面初始化加载
                queryFixtureMaintenanceProfile: '@Url.Action("QueryFixtureMaintenanceProfile", "Fixture")',
                getOrgByParant: '@Url.Action("GetOrgByParant", "Fixture")',
                queryFixtureMaintenanceProfileByUid: '@Url.Action("QueryFixtureMaintenanceProfileByUid", "Fixture")',
                deleteFixtureMaintenanceProfile: '@Url.Action("DeleteFixtureMaintenanceProfile", "Fixture")',
                getMaintenancePlanByFilters: '@Url.Action("GetMaintenancePlanByFilters", "Fixture")',
                //导出excel
                doExportFunction: '@Url.Action("DoFMPExportFunction", "Fixture")',
                //根据OP和功能厂取得治具
                getFixtureNoByFunPlant: '@Url.Action("GetFixtureNoByFunPlant", "Fixture")',
                doAllExportFunction: '@Url.Action("DoAllFMPExportFunction", "Fixture")',
            };

            //#region 定义字段列
            var columns = [
                {
                    createdCell: function (td, cellData, rowData, row, col) {
                        $(td).html('<input type="checkbox" name="cktrans" class="js-checkbox-item" value="' + rowData.Fixture_Maintenance_Profile_UID + '">')
                        .addClass('table-col-checkbox');
                    },
                    className: "text-center"
                },{
                    createdCell: function (td, cellData, rowData, row, col) {
                        var buttonEdit = '<button type="button" class="btn btn-primary btn-xs js-grid-edit" data-id="' + rowData.Fixture_Maintenance_Profile_UID + '">编辑</button>';
                        var result = '<button class="btn btn-primary btn-xs" rel="action-popover">' +
                                    '<i class="fa fa-reorder"></i>' +
                                    '</button>' +
                                    '<div class="hidden popover-content">' +
                                        '{0}' +
                                        '<button type="button" class="btn btn-primary btn-xs js-grid-delete" data-id="' + rowData.Fixture_Maintenance_Profile_UID + '">删除</button>' +
                                    '</div>';
                        result = result.replace('{0}', buttonEdit);
                        $(td).html(result);
                    },
                    className: "text-center"
                },
                {
                    data: null,
                    className: "table-col-seq"
                },
                 {
                     data: "Plant",
                       className: "min-col-xs"
                 },
                 {
                     data: "BG",
                      className: "min-col-xs"
                 },
                {
                    data: "FunPlant",
                    className: "min-col-xs"
                },
                {
                    data: "Fixture_NO",
                    className: "min-col-xs"
                },
                {
                    data: "Maintenance_Type",
                    render: function (data, type, full, meta) {
                        return data == 'D' ? "日常保养" : "周期保养";
                    },
                    className: "min-col-xs"
                },
                {
                    data: "Cycle_ID",
                    className: "min-col-xs"
                },
                {
                    data: "Is_Enable",
                    render: function (data, type, full, meta) {
                        return data ? "启用" : "禁用";
                    },
                    className: "min-col-sm"
                }, {
                    data: "Creator",
                    className: "min-col-xs"
                }, {
                    data: "Created_Date",
                    className: "min-col-xs"
                }, {
                    data: "Modifier",
                    className: "min-col-xs"
                }, {
                    data: "Modified_Date",
                    className: "min-col-xs"
                }];
            //#endregion 定义字段列

            var _getParams = function () {
                
                PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");

                return $('#js_form_query').serialize().replace(/\+/g, " ");

            };

            var _queryFixtureMaintenanceProfile = function (firstLoad, buildCriteria) {

                var config = {
                    pageId: "#page",
                    tableId: "#js_Bom_datatable",
                    remoteUrl: urls.queryFixtureMaintenanceProfile,
                    searchParams: _getParams(),
                    tableOptions: {
                        scrollX: true,
                        autoWidth: true,
                        columns: columns
                    }

                };
                if (!firstLoad) {
                    $('#page').page('destroy');
                }
                if (buildCriteria) {
                    PDMS.Utility.Criteria.Build();
                }
                PDMS.Utility.Pages.Set(config);
            };
            return {
                urls: urls,
                Init: function () {
                    PDMS.Utility.Criteria.Init();
                    _queryFixtureMaintenanceProfile(true, false);
                },
                queryFixtureMaintenanceProfile: function (buildCriteria) {
                    if (!buildCriteria) {
                        buildCriteria = false;
                    }
                    _queryFixtureMaintenanceProfile(false, true);
                }
            }
        })();

        FixtureMaintenanceProfileUrl.Init();

        //隐藏查询框时清空值
        $('#js_search_modal').on('hidden.bs.modal', function (e) {
            $('#js_search_modal').find('input').val('');
        });

        //根据厂区设定OP,并设定初始值---------START
        function SetOpByPlant(Plant_Organization_UID, BG_Organization_UID) {
            $('#js_select_op_types_search').html('<option value=0></option>');
            url = FixtureMaintenanceProfileUrl.urls.getOrgByParant;
            if (Plant_Organization_UID != 0) {
                $.post(url, { Parant_UID: Plant_Organization_UID,type:1 }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_op_types_search').append('<option value="' + data[i].Organization_UID + '">' + data[i].Organization_Name + '</option>');
                    }
                    $('#js_select_op_types_search').val(BG_Organization_UID);
                });
            }
        }


        function SetOpByPlant_Add(Plant_Organization_UID, BG_Organization_UID) {
            $('#js_select_op_types').html('<option value=0></option>');
            url = FixtureMaintenanceProfileUrl.urls.getOrgByParant;
            if (Plant_Organization_UID != 0) {
                $.post(url, { Parant_UID: Plant_Organization_UID, type: 1 }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_op_types').append('<option value="' + data[i].Organization_UID + '">' + data[i].Organization_Name + '</option>');
                    }
                    $('#js_select_op_types').val(BG_Organization_UID);
                });
            }
        }
        //根据厂区设定OP,并设定初始值-----------END

        //根据保养类别设定周期类别代码,并设定初始值---------START
        function SetCycleIdByMaintenanceType(BG_Organization_UID, FunPlant_Organization_UID, Maintenance_Type, Maintenance_Plan_UID) {
            $('#js_select_Cycle_ID').html('<option value=0></option>');
            url = FixtureMaintenanceProfileUrl.urls.getMaintenancePlanByFilters;
            $.post(url, {
                BG_Organization_UID: BG_Organization_UID, FunPlant_Organization_UID: FunPlant_Organization_UID,
                Maintenance_Type: Maintenance_Type
            }, function (data) {
                for (var i = 0; i < data.length; i++) {
                    $('#js_select_Cycle_ID').append('<option value="' + data[i].Maintenance_Plan_UID + '">' + data[i].Cycle_ID + '_' + data[i].Cycle_Interval + '_' + data[i].Cycle_Unit + '</option>');
                }
                $('#js_select_Cycle_ID').selectpicker('refresh');
                $('#js_select_Cycle_ID').selectpicker('val', Maintenance_Plan_UID);
                $("#js_select_Cycle_ID").trigger("liszt:updated");
               // $('#js_select_Cycle_ID').val(Maintenance_Plan_UID);
            });
        }
        //根据保养类别设定周期类别代码,并设定初始值-----------END

        //根据保养类别设定周期类别代码,并设定初始值---------START
        function SetCycleIdByMaintenanceType2(BG_Organization_UID, FunPlant_Organization_UID,Maintenance_Type, Maintenance_Plan_UID) {
            $('#js_select_Cycle_ID_search').html('<option value=0></option>');
            url = FixtureMaintenanceProfileUrl.urls.getMaintenancePlanByFilters;
            $.post(url, {
                BG_Organization_UID: BG_Organization_UID, FunPlant_Organization_UID: FunPlant_Organization_UID,
                Maintenance_Type: Maintenance_Type
