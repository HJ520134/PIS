                                    <label class="col-sm-4 control-label" for="js_input_Machine_ID_query">机台代码</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="Machine_ID" class="form-control input-sm" id="js_input_Machine_ID_query" placeholder="机台代码">
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="col-sm-4 control-label" for="js_input_Equipment_No_query">设备编号</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="Equipment_No" class="form-control input-sm" id="js_input_Equipment_No_query" placeholder="设备编号">
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="col-sm-4 control-label" for="js_input_Machine_Name_query">机台名称</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="Machine_Name" class="form-control input-sm" id="js_input_Machine_Name_query" placeholder="机台名称">
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="col-sm-4 control-label" for="js_Machine_Desc_query">机台描述</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="Machine_Desc" class="form-control input-sm" id="js_Machine_Desc_query" placeholder="机台描述">
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="col-sm-4 control-label" for="js_Machine_Type_query">机台类型</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="Machine_Type" class="form-control input-sm" id="js_Machine_Type_query" placeholder="机台类型">
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="col-sm-4 control-label" for="js_select_line_query">产线</label>
                                    <div class="col-sm-8">
                                        <select class="selectpicker form-control input-sm" id="js_select_line_query" name="Production_Line_UID" data-live-search="true"></select>
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="col-sm-4 control-label" for="js_select_enable_flag_query">是否启用</label>
                                    <div class="col-sm-8">
                                        <select class="selectpicker form-control input-sm" id="js_select_enable_flag_query" name="Is_Enable">
                                            <option value=""></option>
                                            <option value="false">禁用</option>
                                            <option value="true">启用</option>
                                        </select>
                                    </div>
                                </div>
                        </form>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btn-sm" id="js_btn_clear"><i class="fa fa-eraser"></i> @T("Common.Clear")</button>
                    <button type="button" class="btn btn-primary btn-sm" id="js_btn_query"><i class="fa fa-search"></i> @T("Common.Search")</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="js_edit_modal" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Maintain Fixture_Machine</h4>
                </div>

                @using (Html.BeginForm("AddOrEditFixture_Machine", "Fixture", FormMethod.Post, new { id = "js_form_Fixture_Machine_edit" }))
