            //刷新OP类型
            function refreshOptypeAdd() {
                var oporgid = $('#js_select_factory_add option:selected').val();
                url = Fixture_MachineMaintenance.urls.getCurrentOPType;
                $('#js_select_optype_add').empty();
                $('#js_select_optype_add').html('<option></option>');
                if (oporgid != 0) {
                    $.post(url, { plant_OrganizationUID: oporgid }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#js_select_optype_add').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');
                        }
                    });
                }
                $('#js_select_optype_add').selectpicker('refresh');
            }

            //刷新功能厂列表
            function refreshFuncPlant() {
                var factoryVal = $('#js_select_factory_add').val();
                var optypeVal = $('#js_select_optype_add').val()
                var funplantVal = $('#js_select_funplant_add').val();
                if (funplantVal == "" || funplantVal == undefined || funplantVal == false) {
                    funplantVal = 0;
                }
                var getFuncPlantUrl = Fixture_MachineMaintenance.urls.getFunPlantByOPTypes;
                $('#js_select_funplant_add').empty();
                $('#js_select_funplant_add').html('<option></option>');
                $.post(getFuncPlantUrl, { Optype: optypeVal }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_funplant_add').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');
                    }
                });
                $('#js_select_funplant_add').selectpicker('refresh');
            }

            //刷新产线列表
            function refreshProductionLineAdd() {
                $('#js_select_line_add').empty();
                $('#js_select_line_add').html('<option></option>');
                var factoryVal = $('#js_select_factory_add').val();
                var optypeVal = $('#js_select_optype_add').val();
                var funplantVal = $('#js_select_funplant_add').val();
                if (funplantVal == "" || funplantVal == undefined || funplantVal == false) {
                    funplantVal = 0;
                }
                var getProcessUrl = Fixture_MachineMaintenance.urls.getLine_InfoList;
                $.post(getProcessUrl, { Plant_Organization_UID: factoryVal, BG_Organization_UID: optypeVal, FunPlant_Organization_UID: funplantVal }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_line_add').append('<option value="' + data[i].Production_Line_UID + '">' + data[i].Line_ID + "_" + data[i].Line_Name + '</option>');
                    }
                });
                $('#js_select_line_add').selectpicker('refresh');
            }
