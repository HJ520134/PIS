    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
    <script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
    <script type="text/javascript">
        $('#btn_import_fl').on('click', function () {
            $('#js_importExcel_modal').modal('show');
        });
        $('#btn_clear_Update').on('click', function () {
            $('#js_importExcel_modal').modal('hide');
        });
        $(function () {
            var Fixture_MachineMaintenance = (function () {
                var needBuildCriteria = false;
                var urls =
                    {
                        EditFixture_Machine: '@Url.Action("EditFixtureMachine", "Fixture")',
                        AddFixture_Machine: '@Url.Action("AddFixtureMachine", "Fixture")',
                        queryFixture_Machines: '@Url.Action("QueryFixtureMachines", "Fixture")',
                        queryFixture_Machine: '@Url.Action("QueryFixtureMachine", "Fixture")',
                        deleteFixture_Machine: '@Url.Action("DeleteFixtureMachine", "Fixture")',
                        exportFixture_Machines: '@Url.Action("DoExportFixtureMachine", "Fixture")',
                        exportFixture_MachinesByQuery: '@Url.Action("DoExportFixtureMachineByQuery", "Fixture")',
                        //根据厂区取得OP类型
                        getCurrentOPType: '@Url.Action("GetCurrentOPType", "Fixture")',
                        //根据OP类型取得功能厂
                        getFunPlantByOPTypes: '@Url.Action("GetFunPlantByOPTypes", "Fixture")',
                        getLine_InfoList: '@Url.Action("GetProductionLineList", "Fixture")',
                        downloadExcel: '@Url.Action("Fixture_Machine", "Fixture")',
                        //取得设备EMT 顺便带EQP_Uid steven
                        getEquipmentInfoList: '@Url.Action("GetEquipmentInfoList", "Fixture")'
                    };
                var columns = [
                    {
                        createdCell: function (td, cellData, rowData, row, col) {
                            $(td).html('<input type="checkbox" name="cktrans" class="js-checkbox-item" value="' + rowData.Fixture_Machine_UID + '">')
                                 .addClass('table-col-checkbox');
                        },
                        className: "text-center"
                    },
                    {
                        createdCell: function (td, cellData, rowData, row, col) {
                            var hiddenHtml = '<input type="hidden" name="Plant_Organization_UID" value=' + rowData.Plant_Organization_UID + '>'
                                + '<input type="hidden" name="BG_Organization_UID" value="' + rowData.BG_Organization_UID + '">'
                                + '<input type="hidden" name="FunPlant_Organization_UID" value="' + rowData.FunPlant_Organization_UID + '">'
                            + '<input type="hidden" name="EQP_Uid" value="' + rowData.EQP_Uid + '">';

                            var buttonHtml = '<button class="btn btn-primary btn-xs" rel="action-popover">' +
                                            '<i class="fa fa-reorder"></i>' +
                                        '</button>' +
                                        '<div class="hidden popover-content">' +
                                        '<button type="button" class="btn btn-primary btn-xs js-grid-view" data-id="' + rowData.Fixture_Machine_UID + '">查看</button>' +
                                            '<button type="button" class="btn btn-primary btn-xs js-grid-edit" data-id="' + rowData.Fixture_Machine_UID + '">编辑</button>' +
                                            '<button type="button" class="btn btn-primary btn-xs js-grid-delete" data-id="' + rowData.Fixture_Machine_UID + '" data-LineName = "' + rowData.Line_Name + '">删除</button>' +
                                        '</div>';
                            var resultHtml = hiddenHtml + buttonHtml;
                            $(td).html(resultHtml);
                        },
                        className: "text-center"
                    }, {
                        data: null,
                        className: "table-col-seq"
                    }, {
                        data: "Plant_Organization_Name",
                        className: "min-col-xs"
                    }, {
                        data: "BG_Organization_Name",
                        className: "min-col-xs"
                    }, {
                        data: "FunPlant_Organization_Name",
                        className: "min-col-xs"
                    }, {
                        data: "Machine_ID",
                        className: "min-col-xs"
                    }, {
                        data: "Equipment_No",
                        className: "min-col-xs"
                    }, {
                        data: "Machine_Name",
                        className: "min-col-xs"
                    }, {
                        data: "Machine_Desc",
                        className: "min-col-xs"
                    }, {
                        data: "Machine_Type",
                        className: "min-col-xs"
                    }, {
                        data: "Production_Line_Name",
                        className: "min-col-xs"
                    }, {
                        data: "Is_Enable",
                        render: function (data, type, full, meta) {
                            return data ? "启用" : "禁用";
                        },
                        className: "min-col-xs"
                    }
                , {
                    data: "ModifiedUser.User_Name",
                    className: "min-col-lg"
                }, {
                    data: "Modified_Date",
                    className: "min-col-lg"
                }
                ];

                var _getParams = function () {
                    if (needBuildCriteria) {
                        PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                        return $('#js_form_query').serialize().replace(/\+/g, " ");
                    }
                    else {
                        return null;
                    }
                };

                var _queryFixture_Machines = function (firstLoad) {

                    var config = {
                        pageId: "#page",
                        tableId: "#js_Fixture_Machine_datatable",
                        remoteUrl: urls.queryFixture_Machines,
                        searchParams: _getParams(),
                        tableOptions: {
                            scrollX: true,
                            scrollCollapse: false,
                            autoWidth: true,
                            columns: columns
                        }
                    };

                    if (!firstLoad) {
                        $('#page').page('destroy');
                    }

                    if (needBuildCriteria) {
                        PDMS.Utility.Criteria.Build();
                    }

                    PDMS.Utility.Pages.Set(config);
                };

                return {
                    urls: urls,
                    Init: function () {
                        //页面初始化加载查询时，初始化Criteria
                        PDMS.Utility.Criteria.Init();
                        _queryFixture_Machines(true);
                    },
                    queryFixture_Machines: function (isSearchBtn) {
                        needBuildCriteria = (isSearchBtn === true ? true : needBuildCriteria);
                        _queryFixture_Machines(false);
                    }
                }

            })();
            Fixture_MachineMaintenance.Init();

            $('#js_btn_query').click(function () {
                if ($("#js_form_query").valid()) {
                    Fixture_MachineMaintenance.queryFixture_Machines(true);
                    $('#js_search_modal').modal('hide');
                }
            });

            $('#js_btn_export').click(function () {
                var $selectList = $('#js_Fixture_Machine_datatable').find('.js-checkbox-item:checked');
                var len = $selectList.length;
                if (len == 0) {
                    //PDMS.Utility.MessageBox.info('请选择要导出的数据');
                    //全部导出
                    var url = Fixture_MachineMaintenance.urls.exportFixture_MachinesByQuery;
                    //没有查询条件的情况，从查询页面获取
                    if (PDMS.Utility.Settings.Pages.remote.params == null) {
                        PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                    }
                    url += '?' + PDMS.Utility.Settings.Pages.remote.params;
                    window.location.href = url;
                } else {
                    var uids = $.map($selectList, function (row) {
                        return row.value;
                    });
                    var url = Fixture_MachineMaintenance.urls.exportFixture_Machines;
                    url += "?uids=" + uids.toString();
                    window.location.href = url;
                }
            });

            //批量新增设备机台 steven
            $('#js_btn_batchappend').click(function () {
                var url = "@Url.Action("BatchAppend", "Fixture")";
                $.get(url, function (data) {
                    //console.log(data);
                })
            });

            //extra configuration of validate in add/edit model, others via data-attribute
            $('#js_form_Fixture_Machine_edit').validate({
                errorContainer: $('ul.validate-error'),
                errorLabelContainer: $('#js_edit_modal ul.validate-error'),
                wrapper: 'li'
            });

            //clear query conditions in search modal
            $('#js_btn_clear').click(function () {

                PDMS.Utility.Criteria.Clear(function () {
                    $('#js_s_radio_valid').prop('checked', true);
                    $("#js_s_input_reference_date").val(moment().format('YYYY-MM-DD'));
                });
            });

            //do clean up in edit modal show/hide
            $('#js_edit_modal').on('show.bs.modal', function (event) {
                var button = $(event.relatedTarget)
                var modal = $(this)
                var title = modal.find('.modal-title');

                if (button.hasClass('js-grid-edit')) {
                    //编辑
                    title.text("编辑机台信息");
                    $('#js_btn_save_new_Fixture_Machine').hide();
                    $('#js_btn_save_edit_Fixture_Machine').show();
                    $("#js_hidden_edit_id").val(0);

                    var uid = button.attr("data-id");
                    //调api 获取机台信息，填充到编辑画面
                    $.get(Fixture_MachineMaintenance.urls.queryFixture_Machine, { uid: uid }, function (data) {
                        $('#js_select_factory_add').val(data.Plant_Organization_UID);
                        $('#js_select_factory_add').trigger('change');

                        $('#js_select_optype_add').val(data.BG_Organization_UID);
                        $('#js_select_optype_add').trigger('change');

                        $('#js_select_funplant_add').val(data.FunPlant_Organization_UID);
                        $('#js_select_funplant_add').trigger('change');

                        $('#js_input_MachineId_add').val(data.Machine_ID);
                        $('#js_input_EquipmentNo_add').val(data.Equipment_No);
                        $('#js_input_MachineName_add').val(data.Machine_Name);
                        $('#js_input_MachineDesc_add').val(data.Machine_Desc);
                        $('#js_input_MachineType_add').val(data.Machine_Type);
                        $('#js_select_line_add').selectpicker('val', data.Production_Line_UID);
                        $('#js_e_select_enable_flag').selectpicker('val', data.Is_Enable.toString());
                        if (data.EQP_Uid != null) {
                            $('#js_Select_Equipment_add').val(data.EQP_Uid);
                            $('#js_Select_Equipment_add').trigger('change');
                        }
                    });


                } else if (button.attr('id') == 'js_btn_add_Fixture_Machine') {
                    //新增
                    title.text("新增机台");
                    $('#js_select_factory_add').trigger('change');
                    $('#js_select_optype_add').trigger('change');

                    $('#js_select_pixture_MachineId_add').val('');
                    $('#js_input_MachineName_add').val('');
                    $('#js_input_MachineDesc_add').val('');
                    $('#js_input_MachineType_add').val('');
                    $('#js_e_select_enable_flag').selectpicker('val', 'false');

                    //$('#js_input_Fixture_Machine').data('original', '');
                    //$('#js_input_end_date').attr("disabled", false);
                    $('#js_btn_save_new_Fixture_Machine').show();
                    $('#js_btn_save_edit_Fixture_Machine').hide();

                    $('#js_Select_Equipment_add').trigger('change');
                }
            })
            $('#js_edit_modal').on('hidden.bs.modal', function (e) {

                $('#js_edit_modal').find('input').val('');
                $('.list-group.validate-error').empty();
            });

            //edit button in grid
            $('body').on('click', '.js-grid-edit', function () {

                var $sender = $(this);
                if ($sender.hasClass('disabled')) {
                    PDMS.Utility.MessageBox.error("Fixture_Machine already expired, can't edit!");
                    return;
                }

                $("#js_hidden_edit_id").val(1);
                var uid = $(this).attr('data-id'),
                    url = Fixture_MachineMaintenance.urls.queryFixture_Machine;
                $.post(url, { uid: uid }, function (data) {
                    //隐藏字段
                    $('#js_hidden_Fixture_Machine_uid').val(uid);
                    $('#js_hidden_Plant_Organization_uid').val(data.Plant_Organization_UID);
                    $('#js_hidden_BG_Organization_uid').val(data.BG_Organization_UID);
                    $('#js_hidden_FunPlant_Organization_uid').val(data.FunPlant_Organization_UID);
                    //显示字段
                    $('#js_input_Fixture_Machine_Name_add').val(data.Fixture_Machine_Name);
                    $('#js_input_BuildingName').val(data.Building_Name);
                    $('#js_input_FloorName').val(data.Floor_Name);
                });

                $('#js_edit_modal').modal('show', $(this));

            });

            //delete button in grid
            $('body').on('click', '.js-grid-delete', function () {
                var uid = $(this).attr('data-id');
                var dataLineName = $(this).attr('data-LineName');
                PDMS.Utility.MessageBox.confirm(
                    "你确定要删除机台:[" + dataLineName + "]?"
                    , function () {
                        var url = Fixture_MachineMaintenance.urls.deleteFixture_Machine;
                        $.post(url, { uid: uid }, function (data) {

                            if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("机台删除成功", function () {
                                    window.location.reload();
                                });
                            } else if (data == 'HAVEREFERENCE') {
                                PDMS.Utility.MessageBox.error("机台已在使用，不可删除!");
                            } else {
                                PDMS.Utility.MessageBox.error("Error:" + data);
                            }
                        });
                    });
            });

            //查看
            $('body').on('click', '.js-grid-view', function () {

                var uid = $(this).attr('data-id');
                var url = Fixture_MachineMaintenance.urls.queryFixture_Machine;
                $.get(url, { uid: uid }, function (data) {

                    //隐藏字段
                    $('#js_v_input_factory').val(data.Plant_Organization_Name);
                    $('#js_v_input_optype').val(data.BG_Organization_Name);
                    $('#js_v_input_funplant').val(data.FunPlant_Organization_Name);

                    $('#js_v_input_MachineId').val(data.Machine_ID);
                    $('#js_v_input_EquipmentNo').val(data.Equipment_No);
                    $('#js_v_input_MachineName').val(data.Machine_Name);
                    $('#js_v_input_MachineDesc').val(data.Machine_Desc);
                    $('#js_v_input_MachineType').val(data.Machine_Type);

                    $('#js_v_input_LineId').val(data.Production_Line_ID);
                    $('#js_v_input_LineName').val(data.Production_Line_Name);
                    $('#js_v_input_enable_flag').val(data.Is_Enable == true ? "启用" : "禁用");

                    $('#js_v_input_creator').val(data.Created_UserName);
                    $('#js_v_input_create_date').val(data.Created_Date);
                    $('#js_v_input_modifier').val(data.Modified_UserName);
                    $('#js_v_input_modified_date').val(data.Modified_Date);

                    $('#js_view_modal').modal('show');
                });
            });

            firstSearch = true;
            $('#bt_search').click(function () {
                if (firstSearch) {
                    $('#js_select_factory_query').trigger('change');
                    firstSearch = false;
                }

            })

            //查询界面厂区是OP类型变化
            $('#js_select_factory_query').change(function () {
                GetOPTypes();
            })

            $('#js_select_factory_add').change(function () {
                refreshOptypeAdd();
                refreshFuncPlant();
                refreshProductionLineAdd();
                refreshEquipmentAdd();
            })

            //编辑时optype 改变时间
            $('#js_select_optype_add').change(function () {
                refreshFuncPlant();
                refreshProductionLineAdd();
                refreshEquipmentAdd();
            });

            //功能城刷新
            $('#js_select_funplant_add').change(function () {
                refreshProductionLineAdd();
                refreshEquipmentAdd();
            });

            //刷新设备下拉框
            $('#js_Select_Equipment_add').change(function () {
                var equipmentNo = $('#js_Select_Equipment_add').find("option:selected").attr('equipment');
                //当有选中设备时
                if (equipmentNo != '' && equipmentNo != null && equipmentNo != undefined) {
                    //带入设备编号，并禁用
                    $('#js_input_EquipmentNo_add').val(equipmentNo);
                    $('#js_input_EquipmentNo_add').attr("readonly", true);
                } else {
                    //清空设备编号，并启用
                    $('#js_input_EquipmentNo_add').val('');
                    $('#js_input_EquipmentNo_add').attr("readonly", false);
                }
            });


            //当EquipmentNo value on change,the inport EQP_Uid & MachineType . you can import more data
            //$('#js_Select_Equipment_add').change(function () {
            //    if (EqpArray != 0) {
            //        //   alert(JSON.stringify(EqpArray));
            //        var EquipmentNo = $('#js_Select_Equipment_add option:selected').val();
            //        $.each(EqpArray, function (index, values) {
            //            if (EquipmentNo == values.Id) {
            //                $('#js_input_MachineType_add').val(values.valName);
            //                $('#js_hidden_EQP_Uid').val(values.UID);
