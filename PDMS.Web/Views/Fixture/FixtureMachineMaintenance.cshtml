            function GetOPTypes() {
                var oporgid = $('#js_select_factory_query option:selected').val();
                url = Fixture_MachineMaintenance.urls.getCurrentOPType;
                $('#js_select_optype_query').empty();
                $('#js_select_optype_query').html('<option></option>');

                $('#js_select_funplant_query').empty();
                $('#js_select_funplant_query').html('<option></option>');
                if (oporgid != 0) {
                    $.post(url, { plant_OrganizationUID: oporgid }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            if ($('#js_select_optype_query').find('option[value=' + data[i].Organization_UID + ']').length == 0) {
                                $('#js_select_optype_query').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');
                                //$('#js_select_optype_query').selectpicker('refresh');
                            }
                        }
                    });
                }

                $('#js_select_optype_query').selectpicker('refresh');
                $('#js_select_funplant_query').selectpicker('refresh');
            }

            //OP类型变更  start
            $('#js_select_optype_query').change(function () {
                $('#js_select_funplant_query').empty();
                $('#js_select_funplant_query').html('<option></option>');

                $('#js_select_line_query').empty();
                $('#js_select_line_query').html('<option></option>');
                if ($('#js_select_optype_query option:selected').text() != "") {

                    var factoryVal = $('#js_select_factory_query').val();
                    var optypeVal = $('#js_select_optype_query').val()
                    var funplantVal = $('#js_select_funplant_query').val();
                    if (funplantVal == "" || funplantVal == undefined || funplantVal == false) {
                        funplantVal = 0;
                    }
                    var url = Fixture_MachineMaintenance.urls.getFunPlantByOPTypes;
                    $.post(url, { Optype: optypeVal }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#js_select_funplant_query').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');
                            //$('#js_select_funplant_query').selectpicker('refresh');
                        }
                    });

                    if (optypeVal > 0) {
                        var getProcessUrl = Fixture_MachineMaintenance.urls.getLine_InfoList;
                        $.post(getProcessUrl, { Plant_Organization_UID: factoryVal, BG_Organization_UID: optypeVal, FunPlant_Organization_UID: funplantVal }, function (data) {
                            for (var i = 0; i < data.length; i++) {
                                $('#js_select_line_query').append('<option value="' + data[i].Production_Line_UID + '">' + data[i].Line_ID + "_" + data[i].Line_Name + '</option>');
                            }
                        });
                    }
                }
                $('#js_select_funplant_query').selectpicker('refresh');
                $('#js_select_line_query').selectpicker('refresh');
            })

            //功能厂change
            $('#js_select_funplant_query').change(function () {
                url = Fixture_MachineMaintenance.urls.getLine_InfoList;
                $('#js_select_line_query').empty();
                $('#js_select_line_query').html('<option></option>');
                $.post(url, { Plant_Organization_UID: $('#js_select_factory_query').val(), BG_Organization_UID: $('#js_select_optype_query').val(), FunPlant_Organization_UID: $('#js_select_funplant_query').val() }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_line_query').append('<option value="' + data[i].Production_Line_UID + '">' + data[i].Line_ID + "_" + data[i].Line_Name + '</option>');
                    }
                });
                $('#js_select_line_query').selectpicker('refresh');
            });

            //新增修改Fixture_Machine按钮
            $("#js_btn_save_edit_Fixture_Machine").click('click', function () {
                var editform = $('#js_form_Fixture_Machine_edit');
                if (editform.valid()) {
                    var options = {
                        url: Fixture_MachineMaintenance.urls.EditFixture_Machine,
                        type: 'post',
                        success: function (data) {
                            if (data == 'EXIST') {
                                PDMS.Utility.MessageBox.error("修改机台[厂区][OP类型][机台代码][产线代码]重复");
                            }
                            else if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("修改机台信息成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });

            //新增保存Fixture_Machine按钮
            $("#js_btn_save_new_Fixture_Machine").click('click', function () {
                var editform = $('#js_form_Fixture_Machine_edit');
                if (editform.valid()) {
                    var options = {
                        url: Fixture_MachineMaintenance.urls.AddFixture_Machine,
                        type: 'post',
                        success: function (data) {
                            if (data == 'EXIST') {
                                PDMS.Utility.MessageBox.error("新增机台[厂区][OP类型][机台代码][产线代码]重复");
                            }
                            else if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("新增机台信息成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });
            //#region 模板下载
            $('#js_btn_download_fl').on('click', function () {

                this.form.action = Fixture_MachineMaintenance.urls.downloadExcel;
                $('#js_form_excel_add').submit();
            });
            //#endregion
            //提交     stat
            $('#btn_excel_upload').on('click', function () {
                $('#js_form_excel_upload').ajaxSubmit({
                    async: true,
                    beforeSubmit: function () {

                        var fileName = $('#js_s_input_import').val();
                        var fileExtension = fileName.substring(fileName.lastIndexOf('.') + 1);
                        if (fileExtension.toLowerCase() != "xlsx" && fileExtension.toLowerCase() != "xls") {
                            PDMS.Utility.MessageBox.info('请选择Excel文件');
                            $.unblockUI();
                            return false;
                        }
                        if (!$('#js_form_excel_upload').valid()) {
                            $.unblockUI();
                            return false;
                        }
                        $.blockUI({ message: "<h1>@T("QA.Imported")</h1>" });
                    },
                    success: function (data) {
                        $.unblockUI();
                        if (data != '') {
                            PDMS.Utility.MessageBox.error(data);
                        }
                        else {
                            $('#js_importExcel_modal').modal('hide');
                            PDMS.Utility.MessageBox.info('@T("QA.Importsuccessful")', function () {
                                window.location.reload();
                            });
                        }
                    }
                });
            });
            //提交   END
            //隐藏上传窗口时清空值-----------START
            $('#js_importExcel_modal').on('hidden.bs.modal', function (e) {
                $('#js_importExcel_modal').find('input').val('');
            });
            //隐藏上传窗口时清空值-------------END



            //Form Validate
            $('#js_form_Fixture_Machine_edit').validate({
                errorContainer: $('ul.validate-error'),
                errorLabelContainer: $('#js_edit_modal ul.validate-error'),
                wrapper: 'li'
            });
        });
    </script>
}