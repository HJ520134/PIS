                                           data-rule-maxlength="30" data-msg-maxlength="设备编号不能超过 {0} 个字符.">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-sm-5 control-label" for="js_input_MachineName_add">机台名称</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm required" id="js_input_MachineName_add" name="Machine_Name" placeholder="机台名称"
                                           required data-msg-required="请输入[机台名称]."
                                           data-rule-maxlength="30" data-msg-maxlength="机台名称不能超过 {0} 个字符.">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-sm-5 control-label" for="js_input_MachineDesc_add">机台描述</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="js_input_MachineDesc_add" name="Machine_Desc" placeholder="机台描述"
                                           data-rule-maxlength="100" data-msg-maxlength="机台描述不能超过 {0} 个字符.">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-sm-5 control-label" for="js_input_MachineType_add">机台类型</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="js_input_MachineType_add" name="Machine_Type" placeholder="机台类型"
                                           data-rule-maxlength="30" data-msg-maxlength="机台类型不能超过 {0} 个字符.">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-sm-5 control-label" for="js_select_line_add">产线</label>
                                <div class="col-sm-7">
                                    <select class="selectpicker form-control input-sm required" id="js_select_line_add" name="Production_Line_UID" required data-msg-required="[产线]不能为空" data-live-search="true" title="生产线代码_生产线名称"></select>
                                </div>
                            </div>

                            <div class="form-group col-md-6">
                                <label class="col-sm-5 control-label" for="js_e_select_enable_flag">是否启用</label>
                                <div class="col-sm-7">
                                    <select class="selectpicker form-control input-sm" id="js_e_select_enable_flag" name="Is_Enable">
                                        <option value="false">禁用</option>
                                        <option value="true">启用</option>
                                    </select>
                                </div>
                            </div>
                        <!--jquery validata error container-->
                        <div class="col-xs-12"><ul class="list-group validate-error"></ul></div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btn-sm" id="js_btn_save_edit_Fixture_Machine"><i class="fa fa-save"></i> 保存</button>
                    <button type="button" class="btn btn-primary btn-sm" id="js_btn_save_new_Fixture_Machine"><i class="fa fa-save"></i> 保存</button>
                    <button class="btn btn-primary btn-sm" data-dismiss="modal"><i class="fa fa-times"></i> 取消</button>
                </div>
                }
            </div>
        </div>
    </div>
    <div class="modal fade" id="js_importExcel_modal" role="dialog">
