<!-- Main content -->
<section class="content portal-content" style="font-family:微軟正黑體">

    <!--次標題 與 搜尋-->
    <div class="row">
        <!--次標題與Search keyword-->
        <div class="col-md-12 col-lg-9">
            <h4 class="margin-td-5" id="js_search_keywords"></h4>
        </div><!-- /col-次標題與Search keyword -->
        <!--col-右方搜尋與功能按鈕列-->
        <div class="col-md-12 search-field">
            <a href="~/ExcelTemplate/Flowchart_ProductionPlan_Template.xlsx" id="hrfExcelDownload" class="fa fa-download btn btn-primary" target="_blank"><span style="font-family:微軟正黑體"> @T("Common.TeampleDownload")</span></a>
            <a class="fa fa-upload btn btn-primary" id="NewImport" data-toggle="modal" data-target="#js_import_modal"><span style="font-family:微軟正黑體"> @T("Common.ImportExcel")</span></a>
            <a id="js_btn_export" class="fa fa-download btn btn-primary" role="button"><span style="font-family:微軟正黑體"> @T("Common.Export")</span></a>
            <a id="bt_search" class="fa fa-search btn btn-primary" role="button" data-toggle="modal" data-target="#js_search_modal"><span style="font-family:微軟正黑體"> @T("Common.Search")</span></a>
        </div><!-- /col-右方搜尋與功能按鈕列-->
    </div><!--/次標題 與 搜尋-->
    <hr class="hr-custom">

    <!--row Search Keyword collapse-->
    <div class="row">
        <div class="col-xs-12" id="js_criteria_alert_cnt">
        </div>
    </div> <!--/row Search Keyword collapse-->
    <!--內容 表格列-->
    <div class="row">
        <!--表格-->
        <div class="col-md-12 table-container">
            <table class="table table-striped table-hover table-condensed nowrap" id="js_ME_datatable">
                <thead>
                    <tr>
                        <th class="table-col-action nosort">@T("Common.Action")</th>
                        <th>@T("ME.BU")</th>
                        <th>@T("ME.Project_Name")</th>
                        <th>@T("ME.Part_Types")</th>
                        <th>@T("ME.Product_Phase")</th>
                        <th>@T("ME.Plant_Organization_Name")</th>
                        <th>@T("ME.BG_Organization_Name")</th>
                        <th>@T("ME.Daily_Targetoutput")</th>
                        <th>@T("ME.FPY")</th>
                        <th>@T("ME.FlowChart_Version")</th>
                        <th>@T("ME.FlowChart_Version_Comment")</th>
                        <th>@T("ME.Created_User")</th>
                        <th>@T("ME.Created_Date")</th>
                        <th>@T("ME.Modified_User")</th>
                        <th>@T("ME.Modified_Date")</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th class="table-col-action nosort">@T("Common.Action")</th>
                        <th>@T("ME.BU")</th>
                        <th>@T("ME.Project_Name")</th>
                        <th>@T("ME.Part_Types")</th>
