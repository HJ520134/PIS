@section ViewScripts{
    <script>
        $(function () {
            //-----------------------------------------  document.ready 區
            $(document).ready(function () {
                QueryME.Init();
            });
            //-----------------------------------------  全部URL
            var MEUrls = {
                QueryMEs: '@Url.Action("QueryMEs", "ProductionResourcePlan")',
                GetME_Ds: '@Url.Action("GetME_Ds", "ProductionResourcePlan")',
                GetME_ChangeHistory: '@Url.Action("GetME_ChangeHistory", "ProductionResourcePlan")',
            };
            //-----------------------------------------  Grid 清單
            var QueryME = (function () {
                var needBuildCriteria = false;

                var columns = [{
                    createdCell: function (td, cellData, rowData, row, col) {
                        $(td).html('<a tabindex="0" class="btn btn-primary btn-xs" rel="action-popover" role="button"><i class="fa fa-reorder"></i></a>' +
                                    '<div class="hidden popover-content">' +
                                       '<a type="button" class="btn btn-primary btn-sm js-grid-view" data-id="' + rowData.RP_Flowchart_Master_UID + '" role="button" data-toggle="modal" data-target="#js_Process_D_modal">' + '@T("ME.QueryME_D")' + '</a>' +
                                       '<a type="button" class="btn btn-primary btn-sm js-grid-EquipmentMaintenance" data-RP_Flowchart_Master_UID="' + rowData.RP_Flowchart_Master_UID + '" role="button">' + '@T("ME.EquipmentMaintenance")' + '</a>' +
                                       '<a type="button" class="btn btn-primary btn-sm js-grid-changeHistory" data-Plant_Organization_UID="' + rowData.Plant_Organization_UID + '"  data-BG_Organization_UID="' + rowData.BG_Organization_UID + '" data-Project_UID="' + rowData.Project_UID + '" role="button" data-toggle="modal" data-target="#js_changeHistory_model">' + '@T("ME.MS.SearchChangeHistory")' + '</a>' + "<br>" +
                                    '</div>');
                    },
                    className: "text-center"
                }, {
                    data: "BU_D_Name",
                    className: "min-col-sm"
                }, {
                    data: "Project_Name",
                    className: "min-col-sm"
                }, {
                    data: "Part_Types",
                    className: "min-col-sm"
                }, {
                    data: "Product_Phase",
                    className: "min-col-sm"
                }, {
                    data: "Plant_Organization_Name",
                    className: "min-col-sm"
                }, {
                    data: "BG_Organization_Name",
                    className: "min-col-sm"
                }, {
                    data: "Daily_Targetoutput",
                    className: "min-col-sm"
                }, {
                    data: "FPY",
                    className: "min-col-sm text-center"
                }, {
                    data: "FlowChart_Version",
                    className: "min-col-sm"
                }, {
                    data: "FlowChart_Version_Comment",
                    className: "min-col-sm"
                }, {
                    data: "Created_UserName",
                    className: "min-col-sm"
                }, {
                    data: "Created_Date",
                    className: "min-col-sm"
                }, {
                    data: "Modified_UserName",
                    className: "min-col-sm"
                }, {
                    data: "Modified_Date",
                    className: "min-col-sm"
                }
                ];

                var _getParams = function () {
                    if (needBuildCriteria) {
                        return $('#js_form_query').serialize().replace(/\+/g, " ");
                    } else {
                        return null;
                    }
                };

                var _queryME = function (firstLoad) {
                    var config = {
                        pageId: "#page",
                        tableId: "#js_ME_datatable",
                        remoteUrl: MEUrls.QueryMEs,
                        searchParams: _getParams(),
                        tableOptions: {
                            columns: columns
                        }
                    };
                    if (!firstLoad) {
                        $('#page').page('destroy');
                    }
                    if (needBuildCriteria) {
                        PDMS.Utility.Criteria.Build();
                    }
                    PDMS.Utility.Pages.Set(config);
                };

                return {
