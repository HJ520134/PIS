            //显示审核界面---START
            function showStorageTransferApprove(Fixture_Storage_Transfer_UID) {
                var url = StorageTransferSetting.urls.queryStorageTransferByUid;
                $('#js_approve_modal').find('input[name=Fixture_Storage_Transfer_UID]').val(Fixture_Storage_Transfer_UID);
                $.post(url, { Fixture_Storage_Transfer_UID: Fixture_Storage_Transfer_UID }, function (data) {
                    $('#js_approve_modal').find('input[name=Plant]').val(data.Plant);
                    $('#js_approve_modal').find('input[name=BG_Organization]').val(data.BG_Organization);
                    $('#js_approve_modal').find('input[name=FunPlant_Organization]').val(data.FunPlant_Organization);                       
                    $('#js_approve_modal').find('input[name=Part_ID]').val(data.Part_ID);
                    $('#js_approve_modal').find('input[name=Part_Name]').val(data.Part_Name);
                    $('#js_approve_modal').find('input[name=Part_Spec]').val(data.Part_Spec);
                    $('#js_approve_modal').find('input[name=Out_Fixture_Warehouse_ID]').val(data.Out_Fixture_Warehouse_ID);
                    $('#js_approve_modal').find('input[name=Out_Rack_ID]').val(data.Out_Rack_ID);
                    $('#js_approve_modal').find('input[name=Out_Storage_ID]').val(data.Out_Storage_ID);
                    $('#js_approve_modal').find('input[name=In_Fixture_Warehouse_ID]').val(data.In_Fixture_Warehouse_ID);
                    $('#js_approve_modal').find('input[name=In_Rack_ID]').val(data.In_Rack_ID);
                    $('#js_approve_modal').find('input[name=In_Storage_ID]').val(data.In_Storage_ID);
                    $('#js_approve_modal').find('input[name=Transfer_Qty]').val(data.Transfer_Qty);
                    $('#js_approve_modal').find('input[name=Status]').val(data.Status);
                    $('#js_approve_modal').find('input[name=Inventory_Qty]').val(data.Inventory_Qty);
                    if (data.Status == "未审核") {
                        $('#btn_approve_save').removeAttr('disabled');
                    } else {
                        $('#btn_approve_save').attr('disabled', 'disabled');
                    }
                });
            }
            //显示审核界面-----END

            //批量审核 ----START
            $('#btn_approves').click(function () {
                $('#NeedShow').val('1');
                var $selectList = $('#js_Bom_datatable').find('.js-checkbox-item:checked');
                var len = $selectList.length;
                if (len == 0) {
                    PDMS.Utility.MessageBox.info('请先选择未审核的数据');
                } else {
                    $('#transfer_pre').show();
                    $('#transfer_next').show();
                    var Fixture_Storage_Transfer_UID = $('#js_Bom_datatable').find('.js-checkbox-item:checked:eq(0)').val();
                    $('#js_approve_modal').find('input[name=Fixture_Storage_Transfer_UID]').val(Fixture_Storage_Transfer_UID);
                    showStorageTransferApprove(Fixture_Storage_Transfer_UID);
                    $('#js_approve_modal').modal('show');
                    $('#currentID').val(0);

                    $('#js_approve_modal h4').text('储位移转单审核');
                    $('#btn_approve_save').show();
                    $('#transfer_pre').attr('disabled', 'disabled');
                    var Total = $('#js_Bom_datatable').find('.js-checkbox-item:checked').length;
                    if (Total == 1)
                        $('#transfer_next').attr('disabled', 'disabled');
                    else
                        $('#transfer_next').removeAttr('disabled');
                }
            })
            //批量审核 -----END

            //审核上一笔----START
            $('#transfer_pre').click(function () {
                $('#transfer_next').removeAttr('disabled', 'disabled');
                var currentID = $('#currentID').val();
                $('#currentID').val(parseInt(currentID) - 1);
                if (parseInt(currentID) - 1 == 0)
                    $('#transfer_pre').attr('disabled', 'disabled');
                var currentID = $('#currentID').val();
                var Fixture_Storage_Transfer_UID = $('#js_Bom_datatable').find('.js-checkbox-item:checked:eq(' + currentID + ')').val();
                showStorageTransferApprove(Fixture_Storage_Transfer_UID);
            })
            //审核上一笔------END

            //审核下一笔----START
            $('#transfer_next').click(function () {
                $('#transfer_pre').removeAttr('disabled', 'disabled');
                var currentID = $('#currentID').val();
                $('#currentID').val(parseInt(currentID) + 1);
                var Total = $('#js_Bom_datatable').find('.js-checkbox-item:checked').length;
                if (parseInt(currentID) + 2 == parseInt(Total)) {
                    $('#transfer_next').attr('disabled', 'disabled');
                }
                var currentID = $('#currentID').val();
                var Fixture_Storage_Transfer_UID = $('#js_Bom_datatable').find('.js-checkbox-item:checked:eq(' + currentID + ')').val();
                showStorageTransferApprove(Fixture_Storage_Transfer_UID);
            })
            //审核下一笔------END

            //审核返回----START
            $('#transfer_close').click(function () {
                var currentID = $('#js_Bom_datatable').find('.js-checkbox-item:checked').length;
