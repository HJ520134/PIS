                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm date" id="s_input_Applicant_Date_search" name="Applicant_Date">
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Approver_search">审核者</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Approver_search" name="Approver">
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Approver_Date_search">审核日期</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm date" id="s_input_Approver_Date_search" name="Approver_Date">
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Part_ID_search">料号</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Part_ID_search" name="Part_ID">
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Part_Name_search">品名</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm " id="s_input_Part_Name_search" name="Part_Name">
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Part_Spec_search">型号</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Part_Spec_search" name="Part_Spec">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="js_select_status_search">状态</label>
                                <div class="col-sm-7">
                                    <select class="form-control input-sm" id="js_select_status_search" name="Status_UID">
                                        <option></option>
                                        @foreach (var item in Model.FixturePartCreateboundStatus)
                                        {

                                            <option value=@item.Status_UID>@item.Status</option>

                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="js_select_Out_Fixture_Warehouse_Storage_UID_search">转出储位</label>
                                <div class="col-sm-7">
                                    <select class="form-control input-sm" id="js_select_Out_Fixture_Warehouse_Storage_UID_search" name="Out_Fixture_Warehouse_Storage_UID">
                                        <optgroup label="仓库_料架_储位">
                                            <option></option>
                                   
                                        </optgroup>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="js_select_In_Fixture_Warehouse_Storage_UID_search">转入储位</label>
                                <div class="col-sm-7">
                                    <select class="form-control input-sm" id="js_select_In_Fixture_Warehouse_Storage_UID_search" name="In_Fixture_Warehouse_Storage_UID">
