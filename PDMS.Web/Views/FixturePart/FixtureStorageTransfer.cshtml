                                    <input type="text" class="form-control input-sm" id="s_input_Out_Warehouse_ID" name="Out_Fixture_Warehouse_ID" readonly>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Out_Rack_ID">转出料架号</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Out_Rack_ID" name="Out_Rack_ID" readonly>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Out_Storage_ID">转出储位</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Out_Storage_ID" name="Out_Storage_ID" readonly>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Out_Warehouse_ID">转入仓号</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_In_Warehouse_ID" name="In_Fixture_Warehouse_ID" readonly>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Out_Rack_ID">转入料架号</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_In_Rack_ID" name="In_Rack_ID" readonly>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Out_Storage_ID">转入储位</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_In_Storage_ID" name="In_Storage_ID" readonly>
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Inventory_Qty">库存数量</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Inventory_Qty" name="Inventory_Qty" readonly>
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Transfer_Qty">移转数量</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Transfer_Qty" name="Transfer_Qty" readonly>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Status">状态</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Status" name="Status" readonly>
                                </div>
                            </div>

                            <input type="hidden" name="currentID" id="currentID" />
                            <input type="hidden" name="NeedShow" id="NeedShow" />
                            <input type="hidden" name="Fixture_Storage_Transfer_UID" />
                            <div class="col-xs-12"><ul class="list-group validate-error"></ul></div>
                        </div>
                    </div>
                }
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btn-sm" id="transfer_pre"><i class="fa"></i> 上一笔</button>
                    <button type="button" class="btn btn-primary btn-sm" id="transfer_next"><i class="fa"></i> 下一笔</button>
                    <button type="button" class="btn btn-primary btn-sm" id="transfer_close"><i class="fa fa-times"></i> 返回</button>
                    <button type="button" class="btn btn-primary btn-sm" id="btn_approve_save"><i class="fa fa-save"></i> 审核</button>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="js_search_modal" tabindex="-1" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">查询</h4>
                </div>
                <div class="modal-body">
                    <form id="js_form_query" class="form-horizontal" data-need-validate="true">
                        <div class="row">

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Fixture_Storage_Transfer_ID_search">移转单编号</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Fixture_Storage_Transfer_ID_search" name="Fixture_Storage_Transfer_ID">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="js_select_factory_query">厂区</label>
                                <div class="col-sm-7">
                                    <select class="selectpicker form-control input-sm" id="js_select_factory_query" name="Plant_Organization_UID" data-live-search="true">
                                        @foreach (var item in Model.Plants)
                                        {
                                            <option value=@item.Plant_OrganizationUID>@item.Plant</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="js_select_optype_query">OP类型</label>
                                <div class="col-sm-7">
                                    <select class="selectpicker form-control input-sm" id="js_select_optype_query" name="BG_Organization_UID" data-live-search="true"></select>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="js_select_funplant_query">功能厂</label>
                                <div class="col-sm-7">
                                    <select class="selectpicker form-control input-sm" id="js_select_funplant_query" name="FunPlant_Organization_UID" data-live-search="true"></select>
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="s_input_Applicant_search">申请者</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm" id="s_input_Applicant_search" name="Applicant">
