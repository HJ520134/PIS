                //做一个审核的判断
                if (Number($('#js_approve_modal').find('input[name=Transfer_Qty]').val()) < Number($('#js_edit_modal').find('input[name=Inventory_Qty]').val())) {
                    PDMS.Utility.MessageBox.info('转移数量不能大于库存数量');
                    return false;
                }
                url = StorageTransferSetting.urls.approveStorageTransferByUid;
                $.post(url, { Fixture_Storage_Transfer_UID: Fixture_Storage_Transfer_UID }, function (data) {
                    if (data.length > 2) {
                        PDMS.Utility.MessageBox.error(data);
                    } else {
                        PDMS.Utility.MessageBox.info('审核成功');
                        $('#js_approve_modal').find('input[name=Status]').val('已审核');
                        $('#js_approve_modal #btn_approve_save').attr('disabled', 'disabled');
                    }
                })
            })
