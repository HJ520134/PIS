            var currentID = $('#currentID').val();
            var Fixture_Storage_Inbound_UID = $('#js_Bom_datatable').find('.js-checkbox-item:checked:eq(' + currentID + ')').val();
            showInBoundApprove(Fixture_Storage_Inbound_UID);
        })
        //审核下一笔------END

        //审核返回----START
        $('#bound_close').click(function () {
            debugger;
            var currentID = $('#js_Bom_datatable').find('.js-checkbox-item:checked').length;
            $('#currentID').val(currentID);
            $('#js_approve_modal').modal('hide');
        })
        //审核返回------END

        //审核保存---START
        $('#js_btn_approve_save').click(function () {
            debugger;
            var Fixture_Storage_Inbound_UID = $('#js_approve_modal').find('input[name=Fixture_Storage_Inbound_UID]').val();
            url = CreateBoundSetting.urls.approveCreateboundByUid;

            $.post(url, { Fixture_Storage_Inbound_UID: Fixture_Storage_Inbound_UID }, function (data) {
                if (data.length > 2) {
                    PDMS.Utility.MessageBox.error(data);
                } else {
                    PDMS.Utility.MessageBox.info('审核成功');
                    $('#js_approve_modal').find('input[name=Status]').val('已审核');
                    $('#js_approve_modal #js_btn_approve_save').attr('disabled', 'disabled');
                
                    if( $('#ISShow').val()==1)
                    {
                                             
                        $('#js_approve_modal').modal('hide');
                        $('#ISShow').val(0); 
                        CreateBoundSetting.queryCreateBounds();
                    }
                }
            })
        })
        //审核保存-----END

        //审核界面隐藏时判定是否显示新的---START
        $('#js_approve_modal').on('hidden.bs.modal', function (e) {
            $('#bound_pre').hide();
            $('#bound_next').hide();
            CreateBoundSetting.queryCreateBounds();
        })
        //审核界面隐藏时判定是否显示新的-----END

        //审核--start
        $('body').on('click', '.js-grid-approve', function () {
            $('#ISShow').val(1);            
            $('#js_btn_approve_save').show();
            var Fixture_Storage_Inbound_UID = $(this).attr('data-id');
            showInBoundApprove(Fixture_Storage_Inbound_UID);
            $('#js_approve_modal').modal('show');
            $('#js_approve_modal').find('input[name=Fixture_Storage_Inbound_UID]').val(Fixture_Storage_Inbound_UID);
            $('#js_approve_modal h4').text('开账审核');
        });
        //审核--end

        //隐藏编辑框时清空值
        $('#js_edit_modal').on('hidden.bs.modal', function (e) {
            $('#js_edit_modal').find('input').val('');
            $('#js_edit_modal').find('select').val('');
            $('#js_select_warehouse_storage_uid').selectpicker('val', '');
            $('#js_select_warehouse_storage_uid').trigger("liszt:updated");

            $('#js_select_material_uid').selectpicker('val', '');
            $('#js_select_material_uid').trigger("liszt:updated");
            $('.list-group.validate-error').empty();
        });


        //导出按钮
        $('#js_btn_export').click(function () {

            var $selectList = $('#js_Bom_datatable').find('.js-checkbox-item:checked');
            var $selectList2 = $('#js_Bom_datatable').find('.js-checkbox-item2:checked');
            var len = $selectList.length;
            var len2 = $selectList2.length;
            if (len == 0 && len2==0) {
                //全部导出
                var url = CreateBoundSetting.urls.doAllExportCreateBoundReprot;
                //没有查询条件的情况，从查询页面获取
                if (PDMS.Utility.Settings.Pages.remote.params == null) {
                    PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                }
                url += '?' + PDMS.Utility.Settings.Pages.remote.params;
                window.location.href = url;

            } else {
                var uids = $.map($selectList, function (row) {
                    return row.value;
                });
                var uids2 = $.map($selectList2, function (row) {
                    return row.value;
                });
                debugger;
                if (uids != null)
                {
                    uids += "," + uids2
                }
                $('table').find('.js-checkbox-all,.js-checkbox-item,.js-checkbox-item2').prop('checked', false);

                // $('table').find('.js-checkbox-all,.js-checkbox-item').prop('checked', false);
                var url = CreateBoundSetting.urls.doExportCreateBoundReprot;
                url += "?uids=" + uids.toString();
                window.location.href = url;
            }
        });


    })
    </script>
}
