@section ViewScripts{
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
    <script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#bound_pre').hide();
            $('#bound_next').hide();
        })
    $('#js_btn_clear').click(function () {
        PDMS.Utility.Criteria.Clear();
    });

    $(function () {
        var CreateBoundSetting = (function () {
            var urls = {

                //根据厂区取得OP类型
                getCurrentOPType: '@Url.Action("GetCurrentOPType", "Fixture")',
                //根据OP类型取得功能厂
                getFunPlantByOPTypes: '@Url.Action("GetFunPlantByOPTypes", "Fixture")',
                //画面初始化加载
                queryCreateBounds: '@Url.Action("QueryCreateBounds", "FixturePart")',
                //获取储位信息
                getFixtureWarehouseStorages: '@Url.Action("GetFixtureWarehouseStorages", "FixturePart")',
                //获取料号信息
                getFixtureParts: '@Url.Action("GetFixtureParts", "FixturePart")',
                //根据ID获取开账信息
                queryInboundByUid: '@Url.Action("QueryInboundByUid", "FixturePart")',
                //删除开账信息
                deleteCreateBound: '@Url.Action("DeleteCreateBound", "FixturePart")',

                approveCreateboundByUid: '@Url.Action("ApproveCreateboundByUid", "FixturePart")',

                @*queryWarSt: '@Url.Action("QueryWarSt", "FixturePart")',*@

                //导出excel
                doExportCreateBoundReprot: '@Url.Action("DoExportCreateBoundReprot", "FixturePart")',

                doAllExportCreateBoundReprot: '@Url.Action("DoAllExportCreateBoundReprot", "FixturePart")',
          
            };
            //#region 定义字段列
            var columns = [
                {
                    createdCell: function (td, cellData, rowData, row, col) {
                        if (rowData.Status=="未审核")
                        {
                            $(td).html('<input type="checkbox" name="cktrans" class="js-checkbox-item" value="' + rowData.Fixture_Storage_Inbound_UID + '">')
                                        .addClass('table-col-checkbox');
                        } else {
                            $(td).html('<input type="checkbox"  name="cktrans" class="js-checkbox-item2" value="' + rowData.Fixture_Storage_Inbound_UID + '">')
                                        .addClass('table-col-checkbox');
                        }

                    },
                    className: "text-center"
                },{
                    data: null,
                    className: "table-col-seq"
                }, {
                    createdCell: function (td, cellData, rowData, row, col) {
                        var buttonEdit = '<button type="button" class="btn btn-primary btn-xs js-grid-edit" data-id="' + rowData.Fixture_Storage_Inbound_UID + '">编辑</button>';
                        var buttonAppr = '<button type="button" class="btn btn-primary btn-xs js-grid-approve" data-id="' + rowData.Fixture_Storage_Inbound_UID + '">审核</button>';
                        var buttonDelete = '<button type="button" class="btn btn-primary btn-xs js-grid-delete" data-id="' + rowData.Fixture_Storage_Inbound_UID + '">删除</button>';
                        var result = '<button class="btn btn-primary btn-xs" rel="action-popover">' +
                                    '<i class="fa fa-reorder"></i>' +
                                    '</button>' +
                                    '<div class="hidden popover-content">' +
                                        '{0}' +
                                        '{1}' +
                                        '{2}'+
                                        '<button type="button" class="btn btn-primary btn-xs js-grid-view" data-id="' + rowData.Fixture_Storage_Inbound_UID + '">查看</button>' +
                                    '</div>';
                        if (rowData.Status == "未审核") {
                            result = result.replace('{0}', buttonEdit);
                            result = result.replace('{1}', buttonAppr);
                            result = result.replace('{2}', buttonDelete);
                        } else {
                            result = result.replace('{0}', '');
                            result = result.replace('{1}', '');
                            result = result.replace('{2}', '');
                        }
                        $(td).html(result);
                    },
                    className: "text-center"
                }, {
                    data: "Status",
                    className: "min-col-xs"
                }, {
                    data: "Plant",
                    className: "min-col-xs"
                }, {
