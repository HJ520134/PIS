                                            <option value=""></option>
                                            <option value="false">否</option>
                                            <option value="true">是</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="js_s_input_Purchase_Cycle">采购周期(天)</label>
                                    <div class="col-sm-8">
                                        <input type="number" name="Purchase_Cycle" class="form-control input-sm" id="js_s_input_Purchase_Cycle">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="js_s_select_enable_flag">是否启用</label>
                                    <div class="col-sm-8">
