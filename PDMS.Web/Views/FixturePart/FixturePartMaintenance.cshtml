                                    <div class="col-sm-8">
                                        <select class="selectpicker form-control input-sm" id="js_s_select_Is_Automation" name="Is_Automation">
                                            <option value=""></option>
                                            <option value="false">否</option>
                                            <option value="true">是</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
