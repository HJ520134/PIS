                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btn-sm" id="js_btn_clear"><i class="fa fa-eraser"></i> @T("Common.Clear")</button>
                    <button type="button" class="btn btn-primary btn-sm" id="js_btn_query"><i class="fa fa-search"></i> @T("Common.Search")</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="js_edit_modal" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Maintain Fixture_Part</h4>
                </div>

                @using (Html.BeginForm("AddOrEditFixture_Part", "FixturePart", FormMethod.Post, new { id = "js_form_Fixture_Part_edit" }))
                {
                    <div class="modal-body form-horizontal">
                        <div class="row">
                            <input type="hidden" name="Fixture_Part_UID" id="js_hidden_Fixture_Part_uid" value="0" />
                            <input type="hidden" name="Plant_Organization_UID" id="js_hidden_Plant_Organization_uid" value="0" />
                            <input type="hidden" name="BG_Organization_UID" id="js_hidden_BG_Organization_uid" value="0" />
                            <input type="hidden" name="FunPlant_Organization_UID" id="js_hidden_FunPlant_Organization_uid" value="0" />

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_select_factory_add">厂区</label>
                                    <div class="col-sm-7">
                                        <select class="selectpicker form-control input-sm" id="js_select_factory_add" name="Plant_Organization_UID" data-live-search="true">
                                            @foreach (var item in Model.Plants)
                                            {
                                                <option value=@item.Plant_OrganizationUID>@item.Plant</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_select_optype_add">OP类型</label>
                                    <div class="col-sm-7">
                                        <select class="selectpicker form-control input-sm required" id="js_select_optype_add" name="BG_Organization_UID" data-live-search="true" required data-msg-required="请选择[OP类型]."></select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_select_funplant_add">功能厂</label>
                                    <div class="col-sm-7">
                                        <select class="selectpicker form-control input-sm" id="js_select_funplant_add" name="FunPlant_Organization_UID" data-live-search="true"></select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_Part_ID">配件料号</label>
                                    <div class="col-sm-7">
                                        <input type="text" class="form-control input-sm required" id="js_input_Part_ID" name="Part_ID" placeholder="配件料号"
                                               required data-msg-required="请输入[配件料号]."
                                               data-rule-maxlength="20" data-msg-maxlength="配件料号不能超过 {0} 个字符.">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_Part_Name">品名</label>
                                    <div class="col-sm-7">
                                        <input type="text" class="form-control input-sm required" id="js_input_Part_Name" name="Part_Name" placeholder="品名"
                                               required data-msg-required="请输入[品名]."
                                               data-rule-maxlength="50" data-msg-maxlength="品名不能超过 {0} 个字符.">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_Part_Spec">规格</label>
                                    <div class="col-sm-7">
                                        <input type="text" class="form-control input-sm required" id="js_input_Part_Spec" name="Part_Spec" placeholder="规格"
                                               required data-msg-required="请输入[规格]."
                                               data-rule-maxlength="50" data-msg-maxlength="规格不能超过 {0} 个字符.">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_e_select_Is_Automation">是否自动化配件</label>
                                    <div class="col-sm-7">
                                        <select class="selectpicker form-control input-sm" id="js_e_select_Is_Automation" name="Is_Automation">
                                            <option value="false">否</option>
                                            <option value="true">是</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_e_select_Is_Standardized">是否标准件</label>
                                    <div class="col-sm-7">
                                        <select class="selectpicker form-control input-sm" id="js_e_select_Is_Standardized" name="Is_Standardized">
                                            <option value="false">否</option>
                                            <option value="true">是</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_e_select_Is_Storage_Managed">是否库存管控</label>
                                    <div class="col-sm-7">
                                        <select class="selectpicker form-control input-sm" id="js_e_select_Is_Storage_Managed" name="Is_Storage_Managed">
                                            <option value="false">否</option>
                                            <option value="true">是</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_Purchase_Cycle">采购周期(天)</label>
                                    <div class="col-sm-7">
                                        <input type="number" class="form-control input-sm required" id="js_input_Purchase_Cycle" name="Purchase_Cycle" placeholder="采购周期" min="0" step="1"
                                               required data-msg-required="请输入[采购周期]." data-msg-maxlength="采购周期不能小于0.">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_e_select_enable_flag">是否启用</label>
                                    <div class="col-sm-7">
                                        <select class="selectpicker form-control input-sm" id="js_e_select_enable_flag" name="Is_Enable">
                                            <option value="false">禁用</option>
                                            <option value="true">启用</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <!--jquery validata error container-->
                            <div class="col-xs-12"><ul class="list-group validate-error"></ul></div>

                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary btn-sm" id="js_btn_save_edit_Fixture_Part"><i class="fa fa-save"></i> 保存</button>
                        <button type="button" class="btn btn-primary btn-sm" id="js_btn_save_new_Fixture_Part"><i class="fa fa-save"></i> 保存</button>
                        <button class="btn btn-primary btn-sm" data-dismiss="modal"><i class="fa fa-times"></i> 取消</button>
                    </div>
                }
            </div>
        </div>
    </div>
    <div class="modal fade" id="js_importExcel_modal" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">导入治具配件</h4>
                </div>
                @using (Html.BeginForm("ImportFixturePart", "FixturePart", FormMethod.Post, new { enctype = "multipart/form-data", id = "js_form_excel_upload" }))
                {
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-sm-2">
                                <label class="control-label" for="js_s_input_import">选择Excel档</label>
                            </div>
                            <div class="col-sm-8">
                                <input type="file" class="form-control" id="js_s_input_import" name="uploadName" placeholder="选择文件"
                                       required data-msg-required="请选择要上传的Excel档!" accept=".xls,.xlsx" />
                            </div>
                        </div>
                    </div>

                    <div class="modal-footer">

                        <button type="button" class="btn btn-primary btn-sm" id="btn_clear_Update"><i class="fa fa-times"></i>取消</button>
                        <button type="button" class="btn btn-primary btn-sm" id="btn_excel_upload"><i class="fa fa-save"></i>提交</button>

                    </div>
                }
            </div>
        </div>
    </div>
}

@section ViewScripts{
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
    <script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
    <script type="text/javascript">
        $('#btn_import_fl').on('click', function () {
            $('#js_importExcel_modal').modal('show');
        });
        $('#btn_clear_Update').on('click', function () {
            $('#js_importExcel_modal').modal('hide');
        });
        $(function () {
            var Fixture_PartMaintenance = (function () {
                var needBuildCriteria = false;
                var urls =
                    {
                        EditFixture_Part: '@Url.Action("EditFixturePart", "FixturePart")',
                        AddFixture_Part: '@Url.Action("AddFixturePart", "FixturePart")',
                        queryFixture_Parts: '@Url.Action("QueryFixtureParts", "FixturePart")',
                        queryFixture_Part: '@Url.Action("QueryFixturePart", "FixturePart")',
                        deleteFixture_Part: '@Url.Action("DeleteFixturePart", "FixturePart")',
                        exportFixture_Parts: '@Url.Action("DoExportFixturePart", "FixturePart")',
                        exportFixture_PartsByQuery: '@Url.Action("DoExportFixturePartByQuery", "FixturePart")',
                        //根据厂区取得OP类型
                        getCurrentOPType: '@Url.Action("GetCurrentOPType", "Fixture")',
                        //根据OP类型取得功能厂
                        getFunPlantByOPTypes: '@Url.Action("GetFunPlantByOPTypes", "Fixture")'
                    };
                var columns = [
                    {
                        createdCell: function (td, cellData, rowData, row, col) {
                            $(td).html('<input type="checkbox" name="cktrans" class="js-checkbox-item" value="' + rowData.Fixture_Part_UID + '">')
                                 .addClass('table-col-checkbox');
                        },
                        className: "text-center"
                    },
                    {
                        createdCell: function (td, cellData, rowData, row, col) {
                            var hiddenHtml = '<input type="hidden" name="Plant_Organization_UID" value=' + rowData.Plant_Organization_UID + '>'
                                + '<input type="hidden" name="BG_Organization_UID" value="' + rowData.BG_Organization_UID + '">'
                                + '<input type="hidden" name="FunPlant_Organization_UID" value="' + rowData.FunPlant_Organization_UID + '">';

                            var buttonHtml = '<button class="btn btn-primary btn-xs" rel="action-popover">' +
                                            '<i class="fa fa-reorder"></i>' +
                                        '</button>' +
                                        '<div class="hidden popover-content">' +
                                            '<button type="button" class="btn btn-primary btn-xs js-grid-edit" data-id="' + rowData.Fixture_Part_UID + '">编辑</button>' +
                                            '<button type="button" class="btn btn-primary btn-xs js-grid-delete" data-id="' + rowData.Fixture_Part_UID + '" data-Fixture_PartName = "' + rowData.Part_Name + '">删除</button>' +
                                        '</div>';
                            var resultHtml = hiddenHtml + buttonHtml;
                            $(td).html(resultHtml);
                        },
                        className: "text-center"
                    }, {
                        data: null,
                        className: "table-col-seq"
                    }, {
                        data: "Plant_Organization_Name",
                        className: "min-col-xs"
                    }, {
                        data: "BG_Organization_Name",
                        className: "min-col-xs"
                    }, {
                        data: "FunPlant_Organization_Name",
                        className: "min-col-xs"
                    }, {
                        data: "Part_ID",
                        className: "min-col-xs"
                    }, {
                        data: "Part_Name",
                        className: "min-col-xs"
                    }, {
                        data: "Part_Spec",
                        className: "min-col-xs"
                    }, {
                        data: "Purchase_Cycle",
                        className: "min-col-xs"
                    }, {
                        data: "Is_Automation",
                        render: function (data, type, full, meta) {
                            return data ? "是" : "否";
                        },
                        className: "min-col-xs"
                    }, {
                        data: "Is_Standardized",
                        render: function (data, type, full, meta) {
                            return data ? "是" : "否";
                        },
                        className: "min-col-xs"
                    }, {
                        data: "Is_Storage_Managed",
                        render: function (data, type, full, meta) {
                            return data ? "是" : "否";
                        },
                        className: "min-col-xs"
                    }, {
                        data: "Is_Enable",
                        render: function (data, type, full, meta) {
                            return data ? "启用" : "禁用";
                        },
                        className: "min-col-xs"
                    }, {
                        data: "Created_UserName",
                        className: "min-col-lg"
                    }, {
                        data: "Created_Date",
                        className: "min-col-lg"
                    }, {
                        data: "ModifiedUser.User_Name",
                        className: "min-col-lg"
                    }, {
                        data: "Modified_Date",
                        className: "min-col-lg"
                    }
                ];

                var _getParams = function () {
                    if (needBuildCriteria) {
                        PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                        return $('#js_form_query').serialize().replace(/\+/g, " ");
                    }
                    else {
                        return null;
                    }
                };

                var _queryFixture_Parts = function (firstLoad) {
                    var config = {
                        pageId: "#page",
                        tableId: "#js_Fixture_Part_datatable",
                        remoteUrl: urls.queryFixture_Parts,
                        searchParams: _getParams(),
                        tableOptions: {
                            scrollX: true,
                            scrollCollapse: false,
                            autoWidth: true,
                            columns: columns
                        }
                    };

                    if (!firstLoad) {
                        $('#page').page('destroy');
                    }

                    if (needBuildCriteria) {
                        PDMS.Utility.Criteria.Build();
                    }

                    PDMS.Utility.Pages.Set(config);
                };

                return {
                    urls: urls,
                    Init: function () {
                        //页面初始化加载查询时，初始化Criteria
                        PDMS.Utility.Criteria.Init();
                        _queryFixture_Parts(true);
                    },
                    queryFixture_Parts: function (isSearchBtn) {
                        needBuildCriteria = (isSearchBtn === true ? true : needBuildCriteria);
                        _queryFixture_Parts(false);
                    }
                }

            })();
            Fixture_PartMaintenance.Init();

            $('#js_btn_query').click(function () {
                if ($("#js_form_query").valid()) {
                    Fixture_PartMaintenance.queryFixture_Parts(true);
                    $('#js_search_modal').modal('hide');
                }
            });

            $('#js_btn_export').click(function () {
                var $selectList = $('#js_Fixture_Part_datatable').find('.js-checkbox-item:checked');
                var len = $selectList.length;
                if (len == 0) {
                    //PDMS.Utility.MessageBox.info('请选择要导出的数据!');
                    //全部导出
                    var url = Fixture_PartMaintenance.urls.exportFixture_PartsByQuery;
                    //没有查询条件的情况，从查询页面获取
                    if (PDMS.Utility.Settings.Pages.remote.params == null) {
                        PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                    }
                    url += '?' + PDMS.Utility.Settings.Pages.remote.params;
                    window.location.href = url;
                } else {
                    var uids = $.map($selectList, function (row) {
                        return row.value;
                    });
                    var url = Fixture_PartMaintenance.urls.exportFixture_Parts;
                    url += "?uids=" + uids.toString();
                    window.location.href = url;
                }
            });

            //extra configuration of validate in add/edit model, others via data-attribute
            $('#js_form_Fixture_Part_edit').validate({
                errorContainer: $('ul.validate-error'),
                errorLabelContainer: $('#js_edit_modal ul.validate-error'),
                wrapper: 'li'
            });

            //clear query conditions in search modal
            $('#js_btn_clear').click(function () {
                var $searchform = $('#js_form_query');
                $searchform.find("select").each(function () {
                    $(this).find("option").attr("selected", false);
                    $(this).find("option").first().attr("selected", true);
                    $(this).selectpicker('refresh');
                });
                $searchform.find("input").val('');
                PDMS.Utility.Criteria.Clear();
            });

            //do clean up in edit modal show/hide
            $('#js_edit_modal').on('show.bs.modal', function (event) {
                var button = $(event.relatedTarget)
                var modal = $(this)
                var title = modal.find('.modal-title');

                if (button.hasClass('js-grid-edit')) {
                    //编辑
                    title.text("编辑治具配件");
                    $('#js_btn_save_new_Fixture_Part').hide();
                    $('#js_btn_save_edit_Fixture_Part').show();
                    $("#js_hidden_edit_id").val(0);

                    var uid = button.attr("data-id");
                    //调api 获取治具配件信息，填充到编辑画面
                    $.get(Fixture_PartMaintenance.urls.queryFixture_Part, { uid: uid }, function (data) {
                        $('#js_select_factory_add').val(data.Plant_Organization_UID);
                        $('#js_select_factory_add').trigger('change');

                        $('#js_select_optype_add').val(data.BG_Organization_UID);
                        $('#js_select_optype_add').trigger('change');

                        $('#js_select_funplant_add').val(data.FunPlant_Organization_UID);

                        $('#js_input_Part_ID').val(data.Part_ID);
                        $('#js_input_Part_Name').val(data.Part_Name);
                        $('#js_input_Part_Spec').val(data.Part_Spec);
                        $('#js_e_select_Is_Automation').val(data.Is_Automation.toString());
                        $('#js_e_select_Is_Automation').selectpicker('refresh');
                        $('#js_e_select_Is_Standardized').val(data.Is_Standardized.toString());
                        $('#js_e_select_Is_Standardized').selectpicker('refresh');
                        $('#js_e_select_Is_Storage_Managed').val(data.Is_Storage_Managed.toString());
                        $('#js_e_select_Is_Storage_Managed').selectpicker('refresh');
                        $('#js_input_Purchase_Cycle').val(data.Purchase_Cycle);
                        $('#js_e_select_enable_flag').val(data.Is_Enable.toString());
                        $('#js_e_select_enable_flag').selectpicker('refresh');
                    });


                } else if (button.attr('id') == 'js_btn_add_Fixture_Part') {
                    //新增

                    $("#js_select_factory_add").removeAttr("disabled");
                    $("#js_select_optype_add").removeAttr("disabled");
                    $("#js_select_funplant_add").removeAttr("disabled");

                    title.text("新增治具配件");

                    $('#js_select_factory_add').trigger('change');
                    $('#js_select_optype_add').trigger('change');

                    $('#js_input_Part_ID').val('');
                    $('#js_input_Part_Name').val('');
                    $('#js_input_Part_Spec').val('');
                    $('#js_e_select_Is_Automation').val('false');
                    $('#js_e_select_Is_Standardized').val('false');
                    $('#js_e_select_Is_Storage_Managed').val('false');
                    $('#js_input_Purchase_Cycle').val('');
                    $('#js_e_select_enable_flag').val('false');

                    //$('#js_input_Fixture_Part').data('original', '');
                    //$('#js_input_end_date').attr("disabled", false);
                    $('#js_btn_save_new_Fixture_Part').show();
                    $('#js_btn_save_edit_Fixture_Part').hide();
                }
            })
            $('#js_edit_modal').on('hidden.bs.modal', function (e) {

                $('#js_edit_modal').find('input').val('');
                $('.list-group.validate-error').empty();
            });

            //edit button in grid
            $('body').on('click', '.js-grid-edit', function () {

                $("#js_select_factory_add").attr('disabled', 'disabled');
                $("#js_select_optype_add").attr('disabled', 'disabled');
                $("#js_select_funplant_add").attr('disabled', 'disabled');
                var $sender = $(this);
                if ($sender.hasClass('disabled')) {
                    PDMS.Utility.MessageBox.error("Fixture_Part already expired, can't edit!");
                    return;
                }

                $("#js_hidden_edit_id").val(1);
                var uid = $(this).attr('data-id'),
                    url = Fixture_PartMaintenance.urls.queryFixture_Part;
                $.post(url, { uid: uid }, function (data) {
                    //隐藏字段
                    $('#js_hidden_Fixture_Part_uid').val(uid);
                    $('#js_hidden_Plant_Organization_uid').val(data.Plant_Organization_UID);
                    $('#js_hidden_BG_Organization_uid').val(data.BG_Organization_UID);
                    $('#js_hidden_FunPlant_Organization_uid').val(data.FunPlant_Organization_UID);
                    //显示字段
                    $('#js_input_Fixture_PartName').val(data.Fixture_Part_Name);
                    $('#js_input_BuildingName').val(data.Building_Name);
                    $('#js_input_FloorName').val(data.Floor_Name);
                });

                $('#js_edit_modal').modal('show', $(this));

            });

            //delete button in grid
            $('body').on('click', '.js-grid-delete', function () {
                var uid = $(this).attr('data-id');
                var Fixture_PartName = $(this).attr('data-Fixture_PartName');
                PDMS.Utility.MessageBox.confirm(
                    "你确定要删除治具配件:[" + Fixture_PartName + "]?"
                    , function () {

                        var url = Fixture_PartMaintenance.urls.deleteFixture_Part;
                        $.post(url, { uid: uid }, function (data) {

                            if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("治具配件删除成功", function () {
                                    window.location.reload();
                                });
                            } else if (data == 'HAVEREFERENCE') {
                                PDMS.Utility.MessageBox.error("治具配件已在使用，不可删除!");
                            } else {
                                PDMS.Utility.MessageBox.error("Error:" + data);
                            }
                        });
                    });
            });

            firstSearch = true;
            $('#bt_search').click(function () {
                if (firstSearch) {
                    $('#js_select_factory_query').trigger('change');
                    firstSearch = false;
                }
            })

            //查询界面厂区是OP类型变化
            $('#js_select_factory_query').change(function () {
                GetOPTypes();
            })
            $('#js_select_factory_add').change(function () {
                var oporgid = $('#js_select_factory_add option:selected').val();
                url = Fixture_PartMaintenance.urls.getCurrentOPType;
                if (oporgid != 0) {
                    $('#js_select_optype_add').empty();
                    $('#js_select_optype_add').html('<option></option>')
                    $.post(url, { plant_OrganizationUID: oporgid }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#js_select_optype_add').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');
                        }
                    });
                    $('#js_select_optype_add').selectpicker('refresh');
                }
            })

            //编辑时optype 改变时间
            $('#js_select_optype_add').change(function () {
                var url = Fixture_PartMaintenance.urls.getFunPlantByOPTypes;
                //$('#js_select_funplant_add').html('<option></option>');
                //$('#js_select_funplant_query').selectpicker('refresh');

                if ($('#js_select_optype_add option:selected').text() != "") {
                    $('#js_select_funplant_add').empty();
                    $('#js_select_funplant_add').html('<option></option>');
                    $.post(url, { Optype: $('#js_select_optype_add').val() }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#js_select_funplant_add').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');
                        }
                    })
                    $('#js_select_funplant_add').selectpicker('refresh');
                }
            })

            function GetOPTypes() {
                var oporgid = $('#js_select_factory_query option:selected').val();
                url = Fixture_PartMaintenance.urls.getCurrentOPType;
                $('#js_select_optype_query').empty();
                $('#js_select_optype_query').html('<option></option>');

                $('#js_select_funplant_query').empty();
                $('#js_select_funplant_query').html('<option></option>');
                $('#js_select_funplant_query').selectpicker('refresh');
                if (oporgid != 0) {
                    $.post(url, { plant_OrganizationUID: oporgid }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            if ($('#js_select_optype_query').find('option[value=' + data[i].Organization_UID + ']').length == 0) {
                                $('#js_select_optype_query').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');
                            }
                        }
                    });
                }
                $('#js_select_optype_query').selectpicker('refresh');
            }

            //OP类型变更  start
            $('#js_select_optype_query').change(function () {
                var url = Fixture_PartMaintenance.urls.getFunPlantByOPTypes;
                $('#js_select_funplant_query').empty();
                $('#js_select_funplant_query').html('<option></option>');

                if ($('#js_select_optype_query option:selected').text() != "") {
                    $.post(url, { Optype: $('#js_select_optype_query').val() }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#js_select_funplant_query').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');
                        }
                    })
                }
                $('#js_select_funplant_query').selectpicker('refresh');
            })


            //新增修改Fixture_Part按钮
            $("#js_btn_save_edit_Fixture_Part").click('click', function () {

                var editform = $('#js_form_Fixture_Part_edit');
                if (editform.valid()) {
                    var options = {
                        url: Fixture_PartMaintenance.urls.EditFixture_Part,
                        type: 'post',
                        success: function (data) {
                            if (data == 'EXIST') {
                                PDMS.Utility.MessageBox.error("修改治具配件[厂区][OP类型][治具配件料号]重复");
                            }
                            else if (data == 'SUCCESS') {
                                $('#js_edit_modal').modal('hide');
                                PDMS.Utility.MessageBox.info("修改治具配件成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });

            //新增保存Fixture_Part按钮
            $("#js_btn_save_new_Fixture_Part").click('click', function () {
                $('#js_hidden_Plant_Organization_uid').val($('#js_select_factory_add option:selected').val());
                $('#js_hidden_BG_Organization_uid').val($('#js_select_optype_add option:selected').val());
                $('#js_hidden_FunPlant_Organization_uid').val($('#js_select_funplant_add option:selected').val());

                var editform = $('#js_form_Fixture_Part_edit');
                if (editform.valid()) {
                    var options = {
                        url: Fixture_PartMaintenance.urls.AddFixture_Part,
                        type: 'post',
                        success: function (data) {
                            if (data == 'EXIST') {
                                PDMS.Utility.MessageBox.error("新增治具配件[厂区][OP类型][治具配件料号]重复");
                            }
                            else if (data == 'SUCCESS') {
                                $('#js_edit_modal').modal('hide');
                                PDMS.Utility.MessageBox.info("新增治具配件信息成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });
            //#region 模板下载
            $('#js_btn_download_fl').on('click', function () {

                this.form.action = Fixture_PartMaintenance.urls.downloadExcel;
                $('#js_form_excel_add').submit();
            });
            //#endregion
            //提交     stat
            $('#btn_excel_upload').on('click', function () {
                $('#js_form_excel_upload').ajaxSubmit({
                    async: true,
                    beforeSubmit: function () {

                        var fileName = $('#js_s_input_import').val();
                        var fileExtension = fileName.substring(fileName.lastIndexOf('.') + 1);
                        if (fileExtension.toLowerCase() != "xlsx" && fileExtension.toLowerCase() != "xls") {
                            PDMS.Utility.MessageBox.info('请选择Excel文件');
                            $.unblockUI();
                            return false;
                        }
                        if (!$('#js_form_excel_upload').valid()) {
                            $.unblockUI();
                            return false;
                        }
                        $.blockUI({ message: "<h1>@T("QA.Imported")</h1>" });
                    },
                    success: function (data) {
                        $.unblockUI();
                        if (data != '') {
                            PDMS.Utility.MessageBox.error(data);
                        }
                        else {
                            $('#js_importExcel_modal').modal('hide');
                            PDMS.Utility.MessageBox.info('@T("QA.Importsuccessful")', function () {
                                window.location.reload();
                            });
                        }
                    }
                });
            });
            //提交   END
            //隐藏上传窗口时清空值-----------START
            $('#js_importExcel_modal').on('hidden.bs.modal', function (e) {
                $('#js_importExcel_modal').find('input').val('');
            });
            //隐藏上传窗口时清空值-------------END
            //Form Validate
            $('#js_form_Fixture_Part_edit').validate({
                errorContainer: $('ul.validate-error'),
                errorLabelContainer: $('#js_edit_modal ul.validate-error'),
                wrapper: 'li'
            });
        });
    </script>
}