                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="js_s_select_Is_Standardized">是否标准件</label>
                                    <div class="col-sm-8">
                                        <select class="selectpicker form-control input-sm" id="js_s_select_Is_Standardized" name="Is_Standardized">
                                            <option value=""></option>
                                            <option value="false">否</option>
                                            <option value="true">是</option>
                                        </select>
                                    </div>
                                </div>
