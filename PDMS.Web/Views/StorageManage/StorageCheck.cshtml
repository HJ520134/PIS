            $.blockUI({ message: "<h1>导入中，请稍后...</h1>" });
            $('#js_form_excel_upload').ajaxSubmit({
                beforeSubmit: function () {

                    var fileName = $('#js_s_input_import').val();
                    var fileExtension = fileName.substring(fileName.lastIndexOf('.') + 1);
                    if (fileExtension.toLowerCase() != "xlsx" && fileExtension.toLowerCase() != "xls") {
                        PDMS.Utility.MessageBox.info('请选择Excel文件');
                        $.unblockUI();
                        return false;
                    }
                    if (!$('#js_form_excel_upload').valid()) {
                        $.unblockUI();
                        return false;
                    }
                },
                success: function (data) {
                    $.unblockUI();
                    if (data != '') {
                        PDMS.Utility.MessageBox.error(data);
                    }
                    else {
                        $('#js_importExcel_modal').modal('hide');
                        PDMS.Utility.MessageBox.info('上传成功');
                        StorageCheckSetting.queryStorageChecks(true);
                    }
                }
            });
        });

        //隐藏上传窗口时清空值-----------START
        $('#js_importExcel_modal').on('hidden.bs.modal', function (e) {
            $('#js_importExcel_modal').find('input').val('');
        });
        //隐藏上传窗口时清空值-------------END
        $('#btn_approve').click(function () {

            //全部导出
            var url = StorageCheckSetting.urls.approveStorageCheck;
            //没有查询条件的情况，从查询页面获取
            if (PDMS.Utility.Settings.Pages.remote.params == null) {
                PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
            }
            url += '?' + PDMS.Utility.Settings.Pages.remote.params;

            $.post(url, function (data) {
                if (data.length > 2) {
                    PDMS.Utility.MessageBox.error(data);
                } else {
                    PDMS.Utility.MessageBox.info('审核成功');
                    StorageCheckSetting.queryStorageChecks();
             
                }
            })
        });


        // 下载Excel模板
        $('#btn_excel_download').on('click', function () {
            var url = '@Html.Raw(Url.Action("DownloadStorageCheckExcel", "StorageManage"))';
            //this.form.action = url;
            //$('#js_form_excel_download').submit();
            PartType_UID = $('#js_download_modal').find('select[name=PartType_UID]').val();
            if (PartType_UID == "")
            {
                PartType_UID = 0;
            }

            Plant_UID= $('#js_download_modal').find('select[name=Plant_OrganizationUID]').val();
            if (Plant_UID == "")
            {
                Plant_UID = 0;
            }
            BG_Organization_UID= $('#js_download_modal').find('select[name=BG_Organization_UID]').val();
            if (BG_Organization_UID == "")
            {
                BG_Organization_UID = 0;
            }
            FunPlant_Organization_UID= $('#js_download_modal').find('select[name=FunPlant_Organization_UID]').val();
            if (FunPlant_Organization_UID == "")
            {
                FunPlant_Organization_UID = 0;
            }
            Material_Id=  $('#js_download_modal').find('input[name=Material_Id]').val()         
            Material_Name = $('#js_download_modal').find('input[name=Material_Name]').val()
            Material_Types = $('#js_download_modal').find('input[name=Material_Types]').val()
            Warehouse_ID = $('#js_download_modal').find('input[name=Warehouse_ID]').val()
            Rack_ID = $('#js_download_modal').find('input[name=Rack_ID]').val()
            Storage_ID = $('#js_download_modal').find('input[name=Storage_ID]').val()
            url += "?PartType_UID=" + PartType_UID.toString() + "&Material_Id=" + Material_Id
                + "&Material_Name=" + Material_Name + "&Material_Types=" + Material_Types
                + "&Warehouse_ID=" + Warehouse_ID + "&Rack_ID=" + Rack_ID + "&Storage_ID" + Storage_ID
                + "&Plant_UID=" + Plant_UID + "&BG_Organization_UID=" + BG_Organization_UID + "&FunPlant_Organization_UID=" + FunPlant_Organization_UID;
            window.location.href = url;
            
        });
    })
    </script>
}
