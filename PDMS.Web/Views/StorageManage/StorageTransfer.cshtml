            //显示审核界面---START
            function showStorageTransferApprove(Storage_Transfer_UID) {
                var url = StorageTransferSetting.urls.queryStorageTransferByUid;
                $('#js_approve_modal').find('input[name=Storage_Transfer_UID]').val(Storage_Transfer_UID);
                $.post(url, { Storage_Transfer_UID: Storage_Transfer_UID }, function (data) {
                    $('#js_approve_modal').find('input[name=Transfer_Type]').val(data.Transfer_Type);
                    $('#js_approve_modal').find('input[name=Material_Id]').val(data.Material_Id);
