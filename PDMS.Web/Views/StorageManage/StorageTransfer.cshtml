            //隐藏编辑框时清空值
            $('#js_edit_modal').on('hidden.bs.modal', function (e) {
                $('#js_edit_modal').find('input').val('');
                $('#js_edit_modal h4').text('盘点单申请');
                $('#js_select_check_type_uid').val('');
                $('#js_select_In_Warehouse_Storage_UID').html('<optgroup label="仓库_料架_储位"><option></option></optgroup>');
                $('#js_select_In_Warehouse_Storage_UID').selectpicker('refresh');

                $('#js_select_material_uid').html('<optgroup label="料号_品名_型号"><option></option></optgroup>');
                $('#js_select_material_uid').selectpicker('refresh');

                $('#js_select_Out_Warehouse_Storage_UID').html('<optgroup label="仓库_料架_储位"><option></option></optgroup>');
                $('#js_select_Out_Warehouse_Storage_UID').selectpicker('refresh');

                $('.list-group.validate-error').empty();
            });
            //导出按钮
            $('#js_btn_export').click(function () {

                var $selectList = $('#js_Bom_datatable').find('.js-checkbox-item:checked');
                var $selectList2 = $('#js_Bom_datatable').find('.js-checkbox-item2:checked');
                var len = $selectList.length;
                var len2 = $selectList2.length;
                if (len == 0 && len2 == 0) {
                    //全部导出
                    var url = StorageTransferSetting.urls.doAllExportStorageTransferReprot;
                    //没有查询条件的情况，从查询页面获取
                    if (PDMS.Utility.Settings.Pages.remote.params == null) {
                        PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                    }
                    url += '?' + PDMS.Utility.Settings.Pages.remote.params;
                    window.location.href = url;

                } else {
                    var uids = $.map($selectList, function (row) {
                        return row.value;
                    });
                    var uids2 = $.map($selectList2, function (row) {
                        return row.value;
                    });
                    debugger;
                    if (uids != null) {
                        uids += "," + uids2
                    }

                    $('table').find('.js-checkbox-all,.js-checkbox-item,.js-checkbox-item2').prop('checked', false);

                    var url = StorageTransferSetting.urls.doExportStorageTransferReprot;
                    url += "?uids=" + uids.toString();
                    window.location.href = url;
                }
            });

            //#region 下载Excel模板
            $('#btn_download_fl').on('click', function () {
                var url = '@Html.Raw(Url.Action("DownloadStorageTransferExcel", "StorageManage"))';
                this.form.action = url;
                $('#js_form_excel_download').submit();
            });

            $('#btn_excel_upload').on('click', function () {
                $.blockUI({ message: "<h1>导入中，请稍后...</h1>" });
                $('#js_form_excel_upload').ajaxSubmit({
                    beforeSubmit: function () {

                        var fileName = $('#js_s_input_import').val();
                        var fileExtension = fileName.substring(fileName.lastIndexOf('.') + 1);
                        if (fileExtension.toLowerCase() != "xlsx" && fileExtension.toLowerCase() != "xls") {
                            PDMS.Utility.MessageBox.info('请选择Excel文件');
                            $.unblockUI();
                            return false;
                        }
                        if (!$('#js_form_excel_upload').valid()) {
                            $.unblockUI();
                            return false;
                        }
                    },
                    success: function (data) {
                        $.unblockUI();
                        if (data != '') {
                            PDMS.Utility.MessageBox.error(data);
                        }
                        else {
                            $('#js_importExcel_modal').modal('hide');
                            PDMS.Utility.MessageBox.info('上传成功');
                            StorageTransferSetting.queryStorageChecks(true);
                        }
                    }
                });
            });

            //隐藏上传窗口时清空值-----------START
            $('#js_importExcel_modal').on('hidden.bs.modal', function (e) {
                $('#js_importExcel_modal').find('input').val('');
            });
            //隐藏上传窗口时清空值-------------END

        })
    </script>
