@section ViewScripts{
  <script type="text/javascript">
      $(function () {
          var _getDate=function(SearchThisWeek){
              var url = FlowChartPlan.urls.GetDateTime;
              $.post(url,{IsThisWork:SearchThisWeek}, function (data)
              {
                  document.getElementById('js_input_date').innerHTML=data;
              });

          }
          var FlowChartPlan = (function () {
              var urls = {
                  SearchPlanData: '@Url.Action("QueryProcessIEMGData", "FlowChart")',
                  QueryProcessMGDataSingle: '@Url.Action("QueryProcessMGDataSingle", "FlowChart")',
                  QueryProcessIEMGDataSingle: '@Url.Action("QueryProcessIEMGDataSingle", "FlowChart")',
                  ModifyProcessMGData:'@Url.Action("ModifyProcessMGData", "FlowChart")',
                  ModifyProcessIEData:'@Url.Action("ModifyProcessIEMGData", "FlowChart")',
                  backFL: '@Url.Action("FlowChartList", "FlowChart")',
                  GetDateTime:'@Url.Action("GetDateTime", "FlowChart")',
              };

              var contentDatatable = null;
              //region
              var columns = [
                  {
                      data:null,
                      createdCell: function (td, cellData, rowData, row, col) {
                          var buttonEdit = '<button type="button" class="btn btn-default btn-sm js-grid-edit" data-id="' + rowData.Detail_UID + '"  data-shift="' + rowData.ShiftTimeID + '">@ViewBag.Edit</button>';
                          $(td).html(buttonEdit);
                      },
                      className: ""
                  },

                  {
                      data: "Process_seq",
                      className: "min-col-xs "
                  }, {
                      data: "Process",
                      className: "min-col-xs "
                  }, {
                      data: "Color",
                      className: "min-col-xs "
                  },
                  {
                      data: "Place",
                      className: "min-col-xs "
                  },{
                      data:null,
                      createdCell: function (td, cellData, rowData, row, col) {
                          var result;
                          if(rowData.ShiftTimeID==4){
                              result="白班";
                          }else{
                              result="夜班";
                          }
                          $(td).html(result);
                      },
                      className: ""
                  },
                  {
                      data: "MondayIE_TargetEfficacy",
                      className: "min-col-xs ",
                  },
                   {
                       data: "MondayIE_DeptHuman",
                       className: "min-col-xs ",
                   },
                  {
                      data: "TuesdayIE_TargetEfficacy",
                      className: "min-col-xs ",
                  },
                  {
                      data: "TuesdayIE_DeptHuman",
                      className: "min-col-xs ",
                  },


                  {
                      data: "WednesdayIE_TargetEfficacy",
                      className: "min-col-xs ",
                  }, {
                      data: "WednesdayIE_DeptHuman",
                      className: "min-col-xs ",
                  },

                  {
                      data: "ThursdayIE_TargetEfficacy",
                      className: "min-col-xs ",
                  }, {
                      data: "ThursdayIE_DeptHuman",
                      className: "min-col-xs ",
                  },

                  {
                      data: "FridayIE_TargetEfficacy",
                      className: "min-col-xs ",
                  }, {
                      data: "FridayIE_DeptHuman",
                      className: "min-col-xs ",
                  },

                  {
                      data: "SaterdayIE_TargetEfficacy",
                      className: "min-col-xs ",
                  }, {
                      data: "SaterdayIE_DeptHuman",
                      className: "min-col-xs ",
                  },


                  {
                      data: "SundayIE_TargetEfficacy",
                      className: "min-col-xs ",
                  }, {
                      data: "SundayIE_DeptHuman",
                      className: "min-col-xs ",
                  },

              ];

              //endregion
              var _getParams = function (SearchThisWeek) {
                  return { MasterUID: @ViewBag.MasterUId, IsThisWork: SearchThisWeek };
              };

              var _queryFLS = function (SearchThisWeek) {

                  var config1 = {
                      pageId: "#page",
                      tableId: "#js_Plan_datatable",
                      remoteUrl: urls.SearchPlanData,
                      searchParams: _getParams(SearchThisWeek),
                      tableOptions: {
                          scrollX: true,
                          autoWidth: true,
                          ordering: false,
                          columns: columns
                      }
                  };

                  var config2 = {
                      pageId: "#page",
                      tableId: "#js_Plan_datatable",
                      remoteUrl: urls.SearchPlanData,
                      searchParams: _getParams(SearchThisWeek),
                      tableOptions: {
                          scrollX: true,
                          autoWidth: true,
                          ordering: false,
                          columns: columns
                      }
                  };

                  if(SearchThisWeek)
                  {
                      PDMS.Utility.Pages.Set(config1);
                  }
                  else
                  {
                      PDMS.Utility.Pages.Set(config2);
                  }
              };

              var SetTitle=function()
              {
                  var title = '@ViewBag.CustomerName' + ' ' + '@ViewBag.ProjectName' + ' ' + '@ViewBag.PartTypes' + ' ' + '@ViewBag.ProductPhase' + 'IE人力效率维护'
                  $('#lblTitle').text(title);
              };

              return {
                  urls: urls,
                  Init: function () {
                      $('#page').page('destroy');

                      _getDate(true);

                      var uiNextBtn =document.getElementById("js_btn_searchNextWorkPlan");
                      uiNextBtn.disabled=false;

                      var uiThisBtn =document.getElementById("js_btn_searchthisWorkPlan");
                      uiThisBtn.disabled=true;

                      _queryFLS(true);
                      SetTitle();
                  },
                  QueryFlowChartNextPlan: function () {

                      $('#page').page('destroy');

                      _getDate(false);

                      var uiNextBtn =document.getElementById("js_btn_searchNextWorkPlan");
                      uiNextBtn.disabled=true;

                      var uiThisBtn =document.getElementById("js_btn_searchthisWorkPlan");
                      uiThisBtn.disabled=false;

                      _queryFLS(false);
                      SetTitle();
                  },
                  GetDataTables: function () {
                      if (contentDatatable == null) {
                          contentDatatable = $('#js_Plan_datatable').DataTable({
                              paging: false,
                              searching: false,
                              ordering: false,
                              retrieve: true,
                              columns: columns,
                          });
                      }
                      return contentDatatable;
                  }
              }
          })();

          FlowChartPlan.Init();

          $('#btn_back').on('click', function () {
              var url = FlowChartPlan.urls.backFL;
              window.location.href = url;
          });

          jQuery.validator.addMethod("percent", function (value, element) {
              var reg = /^((\d+\.?\d*)|(\d*\.\d+))\%$/;
              var result = reg.test(value);
              return result;
          }, "@ViewBag.JustInputPercent");


          $('#js_btn_searchthisWorkPlan').on('click',function()
          {
              FlowChartPlan.Init();
          });
          $('#js_btn_searchNextWorkPlan').on('click',function()
          {
              FlowChartPlan.QueryFlowChartNextPlan();
          });


          //#region  div弹出编辑画面

          //#region 点击DataTable弹出div并加载信息
          $('body').on('click', '.js-grid-edit', function () {

              var Detail_UID = $(this).attr('data-id');
              var shift_ID = $(this).attr('data-shift');

