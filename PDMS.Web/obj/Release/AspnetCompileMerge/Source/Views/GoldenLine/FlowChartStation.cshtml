                                <label class="col-sm-5 control-label">Is Birth</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsBirth" value="true" name="IsBirth">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label">Is Output</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsOutput" value="true" name="IsOutput">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label">Is Test</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsTest" value="true" name="IsTest">
                                </div>
                            </div>

                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label">Is GoldenLine</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsGoldenLine" value="true" name="IsGoldenLine">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label">Is OEE</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsOEE" value="true" name="IsOEE">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6" >
                                <label class="col-sm-5 control-label">Is MES</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsOne" value="true" name="IsOne">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6" style="display:none">
                                <label class="col-sm-5 control-label">Is Two</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsTwo" value="true" name="IsTwo">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6" style="display:none">
                                <label class="col-sm-5 control-label">Is Three</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsThree" value="true" name="IsThree">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6" style="display:none">
                                <label class="col-sm-5 control-label">Is Four</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="Is Four" value="true" name="Is Four">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6" style="display:none">
                                <label class="col-sm-5 control-label">Is Five</label>
                                <div class="col-sm-7">
                                    <input type="checkbox" id="IsFive" value="true" name="IsFive">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label">OEE Target</label>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control input-sm required" name="DashboardTarget" placeholder="OEE Target">
                                </div>
                            </div>
                            <div class="form-group col-xs-12 col-md-6 col-lg-6">
                                <label class="col-sm-5 control-label" for="js_input_Is_Enable_Add">@T("Common.Published")</label>
                                <div class="col-sm-7">
                                    <select class="selectpicker form-control input-sm required" id="js_input_Is_Enable" name="IsEnabled" data-live-search="true">
                                       
                                          <option value="true">启用</option>
                                        <option value="false">禁用</option>
                                    </select>
                                </div>
                            </div>
                            <input type="hidden" name="StationID" value="0" />
                            <input type="hidden" name="Plant_Organization_UID" value="@Model.Plant_Organization_UID" />
                            <input type="hidden" name="BG_Organization_UID" value="@Model.BG_Organization_UID" />
                            <input type="hidden" name="FunPlant_Organization_UID" value="@Model.FunPlant_Organization_UID" />
                            <input type="hidden" name="LineID" value="@Model.LineID" />
                            <input type="hidden" name="CustomerID" value="@Model.CustomerID" />
                            <input type="hidden" name="isEdit" value="false" />
                            <!--jquery validata error container-->
                            <div class="col-xs-12"><ul class="list-group validate-error"></ul></div>
                        </div>
                    </div>
                
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal"><i class="fa fa-times"></i> @T("Common.Cancel")</button>
                    <button type="button" class="btn btn-primary btn-sm" id="js_btn_save_new"><i class="fa fa-save"></i> @T("Common.Save")</button>
                </div>
                    }
            </div>
        </div>
    </div>


