@section ViewScripts{
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
    <script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
    <script type="text/javascript">

        $(function () {
            var dataSource = (function () {
                var urls = {
                    getStations: '@Url.Action("GetStation", "GoldenLine")',
                    getStationsByID: '@Url.Action("GetStationByID", "GoldenLine")',
                    backFL: '@Url.Action("LineSetup", "GoldenLine")',
                    removeStationByID: '@Url.Action("RemoveStationByID", "GoldenLine")',
                    getStationsDetectionPointByID:'@Url.Action("GetStationsDetectionPointByID", "GLQAReport")'
                }
                var columns = [
                    {
                        createdCell: function (td, cellData, rowData, row, col) {
                            var buttonEdit = '<button type="button" class="btn btn-primary btn-xs js-grid-edit" data-id="' + rowData.StationID + '">@ViewBag.Edit</button>';
                            var result = '<button class="btn btn-primary btn-xs" rel="action-popover">' +
                                '<i class="fa fa-reorder"></i>' +
                                '</button>' +
                                '<div class="hidden popover-content">' +
                                '{0}' +
                                '{1}' +
                                '{2}' +
                                '<button type="button" class="btn btn-primary btn-xs js-grid-delete" data-id="' + rowData.StationID + '">@ViewBag.Delete</button>' +
                                '</div>';

                            var buttonDetectionPoint = '<button type="button" class="btn btn-primary btn-xs js-grid-DetectionPoint" data-id="' + rowData.StationID + '">检测点维护</button>';
                            var buttonTargetYield = '<button type="button" class="btn btn-primary btn-xs js-grid-TargetYield" data-id="' + rowData.StationID + '">目标良率维护</button>';
                            result = result.replace('{0}', buttonEdit);
                            if(rowData.IsTest)
                            {
                                result = result.replace('{1}', buttonDetectionPoint);
                                result = result.replace('{2}', buttonTargetYield);
                            }else
                            {
                                result = result.replace('{1}', "");
                                result = result.replace('{2}', "");
                            }
                      
                            $(td).html(result);
                        },
                        className: "min-col-xs text-center"
                    }, {
                        className: "table-col-seq"
                    }, {
                        data: 'Seq',
                        className: "min-col-xs text-center"
                    },
                    {
                        data: 'StationName',
                        className: "min-col-xs text-center"
                    }, 
                    {
                        data: 'MESStationName',
                        className: "min-col-xs text-center"
                    },{
                        data: 'Binding_Seq',
                        className: "min-col-xs text-center"
                    }, {
                        data: 'CycleTime',
                        className: "min-col-xs text-center"
                    }, {
                        data: 'IsBirth',
                        render: function (data, type, full, meta) {
                            return data ? 'Y' : 'N';
                        },
                        className: "min-col-xs text-center"
                    }, {
                        data: "IsOutput",
                        render: function (data, type, full, meta) {
                            return data ? 'Y' : 'N';
                        },
                        className: "min-col-xs text-center"
                    }, {
                        data: 'IsTest',
                        render: function (data, type, full, meta) {
                            return data ? 'Y' : 'N';
                        },
                        className: "min-col-xs text-center"
                    },  {
                        data: "IsGoldenLine",
                        render: function (data, type, full, meta) {
                            return data ? 'Y' : 'N';
                        },
                        className: "min-col-xs text-center"
                    }, {
                        data: 'IsOEE',
                        render: function (data, type, full, meta) {
                            return data ? 'Y' : 'N';
                        },
                        className: "min-col-xs text-center"
                    },
                     {
                         data: 'IsOne',
                         render: function (data, type, full, meta) {
                             return data ? 'Y' : 'N';
                         },
                         className: "min-col-xs text-center"
                     },
                        {
                            data: 'DashboardTarget',
                            className: "min-col-xs text-center"
                        }, 
                     {
                         data: "IsEnabled",
                         render: function (data, type, full, meta) {
                             return data ? 'Y' : 'N';
                         },
                         className: "text-center"
                     },
                    {
                        data: 'System_Users.User_Name',
                        className: "min-col-xs text-center"
                    }, {
                        data: "Modified_Date",
                        className: "min-col-xs text-center"
                    }]
                var getStations = function () {
                    var config = {
                        pageId: "#page",
                        tableId: "#js_line_datatable",
                        searchParams: { LineID: @Model.LineID},
                        remoteUrl: urls.getStations,
                        tableOptions: {
                            scrollX: true,
                            scrollCollapse: false,
                            autoWidth: true,
                            columns: columns
                        }
                    }
                    PDMS.Utility.Pages.Set(config);
                }
                return {
                    urls: urls,
                    Init: function () {
                        getStations();
                        $(".text-center").removeClass("sorting");
                        $('.text-center').removeAttr('onclick');
                    },
                    getStations: function () {
                        $('#page').page('destroy');
                        getStations();
                    }
                }
            })();
            dataSource.Init();
            $('#btn_back').on('click', function () {
                var url = dataSource.urls.backFL;
                window.location.href = url;
            });

            $('body').on('click', '.js-grid-delete', function () {
                var id = $(this).attr('data-id'),
                    url = dataSource.urls.removeStationByID;
                if (id) {
                    PDMS.Utility.MessageBox.confirm("Are you sure to delete this item?", function () {
                        $.post(url, { id: id }, function (data) {
                            if (data && data.Result)
                                dataSource.getStations();
                            else
                                PDMS.Utility.MessageBox.error("站点已在使用中或者使用过因此不能删除，如果不用此站点，请在编辑中禁用！");
                                //PDMS.Utility.MessageBox.error("Remove Failed!");
                        });
                    });
                }
            });
            $('#js_btn_add').click(function () {
                $('#isEdit').val(false);
                Seteditadd();
            });
            //新增保存----start
            $('#js_btn_save_new').click(function () {
                $('#js_form_user_edit').ajaxSubmit({
                    beforeSubmit: function () {

                        if ($('#js_edit_modal').find('input[name=StationName]').val() == "") {
                            PDMS.Utility.MessageBox.info('请填写站点名称');
                            return false;
                        }
                        if ($('#js_edit_modal').find('input[name=MESStationName]').val() == "") {
                            PDMS.Utility.MessageBox.info('请填写MES站点名称');
                            return false;
                        }
                        if ($('#js_edit_modal').find('input[name=Binding_Seq]').val() == "") {
                            PDMS.Utility.MessageBox.info('请填写绑定序号');
                            return false;
                        }

                        if ($('#js_edit_modal').find('input[name=CycleTime]').val() == "") {
                            PDMS.Utility.MessageBox.info('请填写CycleTime');
                            return false;
                        }
                        ///用逗号拆分目标，验证用户填写的数据是否满足要求
                        var  OEETarget=$('#js_edit_modal').find('input[name=DashboardTarget]').val();
                        if(OEETarget !="")  //不填写的时候放它一马
                        {
                            var TargetList=OEETarget.split(',');
                            //判断拆分后的数据是否为两个（0~100）的整数
                            if(TargetList.length!=2)
                            {
                                PDMS.Utility.MessageBox.info('OEE Target填写格式错误，请用逗号隔开目标方式填写');
                                return false;
                            }
                            else 
                            {
                                var  Target1=parseInt( TargetList[0]);
                                var  Target2=parseInt(TargetList[1]);
                                //  判断两个目标是否都为数字，且在（0~100）之间

                                if(isNaN(Target1) || isNaN(Target2))
                                {
                                    PDMS.Utility.MessageBox.info('OEE Target填写格式错误，请在逗号两侧使用整数填写');
                                    return false;
                                }
                                else
                                {
                                    if(Target1>=Target2)
                                    {
                                        PDMS.Utility.MessageBox.info('OEE Target填写格式错误，前面的值需要小于后面的值');
                                        return false;
                                    }
                                    else 
                                    {
                                        if ( Target1<0 || Target1>100 || Target2<0 || Target2>100)
                                        {
                                            PDMS.Utility.MessageBox.info('OEE Target填写格式错误，目标值必须在0~100之间');
                                            return false;
                                        }
                                    }
                                }
                            }

                        }

                    },
                    success: function (data) {

                        if (data.length > 4) {
                            PDMS.Utility.MessageBox.info(data);
                        }
                        else {
                            $('#js_edit_modal').modal('hide');
                            PDMS.Utility.MessageBox.info('保存成功');
                            dataSource.getStations();
                        }
                    }
                });
            })
