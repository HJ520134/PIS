            function ShowOEEMetricsGaugeEchart(name, MachineInfo_UID, FirstTarget, SecondTarget, OeeStationValue) {
                OeeValue = OeeStationValue;
                var OEEMetricsGaugeEchart = echarts.init(document.getElementById(name));
                OEEMetricsGaugeEchart.showLoading({ text: 'Trying to read the data...', maskcolor: 'transparent' });
                option = {
                    title: {
                        text: 'OEE Value',
                        x: 'center',
                        y: '0%'
                    },
                    tooltip: {
                        formatter: "{a} <br/>{b} : {c}%"
                    },
                    toolbox: {
                        feature: {
                            restore: {},
                            saveAsImage: {}
                        }
                    },
                    series: [
                        {
                            y: '10%',
                            name: 'OEE Value',
                            type: 'gauge',
                            detail: { formatter: OeeValue, fontSize: 14 },
                            axisLine: {            // 坐标轴线
                                lineStyle: {       // 属性lineStyle控制线条样式
                                    color: [[FirstTarget, '#c23531'], [SecondTarget, '#63869e'], [1, '#91c7ae']]
                                }
                            },
                            data: [{ value: OeeValue, name: '稼动率', MachineID: MachineInfo_UID }]
                        }
                    ]
                };
                var reg = new RegExp("%");
                if (OeeValue == "" || OeeValue == undefined) {
                    option.series[0].data[0].value = 0;
                } else {
                    option.series[0].data[0].value = OeeValue.replace(reg, "");
                }

                OEEMetricsGaugeEchart.setOption(option, true);
                OEEMetricsGaugeEchart.hideLoading();

                OEEMetricsGaugeEchart.on('click', function (param) {
                    console.log(param.data.MachineID);
                    checkSerchParam();
                    if (CkeckIsOK) {
                        //window.open(GetURL(MachineInfo_UID));
                        var reportUrl = '@Html.Raw(Url.Action("oee_metricsphotoreport", "OEE"))';
                        reportUrl = reportUrl + "?isFromPhotoReport=" + "PieOEE" + "&&MachineName=" + param.data.MachineID;
                        window.open(reportUrl);
                        //window.location.href = GetURL(MachineInfo_UID);
                    }
                });
            }

            function ShowAllStationOEE(num) {
                $("#id_PageSize").val(9);
                $("#id_PageNum").val(num);
                var _getParams = function () {
                    return $('#js_form_query').serialize().replace(/\+/g, " ");
                };
                $.ajax({
                    url: MachinePieReport.urls.GetMachinePieReportData,
                    type: 'post',
                    async: 'false',
                    data: _getParams(),
                    dataType: 'json',
                    success: function (result) {
                        for (var i = 0; i < 9; i++) {
                            var index = i + 1;
                            var id_div = "id_div" + index;
                            id_div = "#" + id_div;
                            $(id_div).css("display", "none");
                            var id_all_div = "id_all_div" + index;
                            id_all_div = "#" + id_all_div;
                            $(id_all_div).css("display", "none");
                        };
                        var exportDTCode = false;
                        var exportDFCode = false;
                        for (var i = 0; i < result.length; i++) {
                            if (result[i].IsTotalStationStatic == "NO") {
                                var index = i + 1;
                                var id = "OEEMetricsGaugeEchart" + index;
                                var id_MachineName = "id_MachineName" + index;
                                var id_AvailableRate = "id_AvailableRate" + index;
                                var id_PerformanceRate = "id_PerformanceRate" + index;
                                var id_FirstYield = "id_FirstYield" + index;
                                var id_MachineID = "id_MachineID" + index;
                                var id_div = "id_div" + index;
                                id_div = "#" + id_div;
                                $(id_div).css("display", "flex");
                                $("#id_div_lastUpdateTime").css("display", "block");
                                $("#id_div_Abnormal").css("display", "block");
                                //总体的div
                                var id_all_div = "id_all_div" + index;
                                id_all_div = "#" + id_all_div;
                                $(id_all_div).css("display", "block");

                                //异常状态
                                var id_dtcode_div = "id_dtcode_div" + index;
                                id_dtcode_div = "#" + id_dtcode_div;
                                if (result[i].Is_DtCodeAbnormal) {
                                    exportDTCode = true;
                                    $(id_dtcode_div).addClass("isflash_dtcode");
                                } else {
                                    $(id_dtcode_div).removeClass("isflash_dtcode");
                                }
                                var id_cnc_div = "id_cnc_div" + index;
                                id_cnc_div = "#" + id_cnc_div;
                                if (result[i].Is_CncResetAbnomal) {
                                    $(id_cnc_div).addClass("isflash_cnc");
                                } else {
                                    $(id_cnc_div).removeClass("isflash_cnc");
                                }

                                var id_oeevalue_div = "id_oeevalue_div" + index;
                                id_oeevalue_div = "#" + id_oeevalue_div;
                                if (result[i].Is_OeeAbnomal) {
                                    $(id_oeevalue_div).addClass("isflash_oee");
                                } else {
                                    $(id_oeevalue_div).removeClass("isflash_oee");
                                }

                                //DF Code
                                var id_dfcode_div = "id_dfcode_div" + index;
                                id_dfcode_div = "#" + id_dfcode_div;
                                if (result[i].Is_DfCeodeAbnomal) {
                                    exportDFCode = true;
                                    $(id_dfcode_div).addClass("isflash_dfcode");
                                } else {
                                    $(id_dfcode_div).removeClass("isflash_dfcode");
                                }

                                //是否断网

                                var id_OffLine = "id_OffLine" + index;
                                id_OffLine = "#" + id_OffLine;
                                var id_OnLine = "id_OnLine" + index;
                                id_OnLine = "#" + id_OnLine;
                                if (result[i].Is_ShowOffLine) {
                                    if (result[i].Is_OffLine) {
                                        $(id_OffLine).css("display", "block");
                                        $(id_OffLine).css({ "background-color": "#ff0000" });
                                        $(id_OffLine).addClass("isOffLine");
                                        $(id_OnLine).css("display", "none");
                                    }
                                    else {
                                        $(id_OnLine).css("display", "block");
                                        $(id_OffLine).css("display", "none");
                                        $(id_OffLine).removeClass("isOffLine");
                                    }
                                }else
                                {
                                    $(id_OnLine).css("display", "none");
                                    $(id_OffLine).css("display", "none");
                                }


                                id_MachineName = "#" + id_MachineName;
                                id_AvailableRate = "#" + id_AvailableRate;
                                id_PerformanceRate = "#" + id_PerformanceRate;
                                id_FirstYield = "#" + id_FirstYield;
                                id_MachineID = "#" + id_MachineID;
                                $(id_MachineID).val(result[i].OEE_MachineInfo_UID);
                                var MachineInfo_UID = result[i].OEE_MachineInfo_UID;
                                $(id_MachineName).text(result[i].MachineName);
                                $(id_AvailableRate).text((result[i].AvailableRate * 100).toFixed(2) + "%");
                                $(id_PerformanceRate).text((result[i].PerformanceRate * 100).toFixed(2) + "%");
                                $(id_FirstYield).text((result[i].FirstYield * 100).toFixed(2) + "%");
                                ShowOEEMetricsGaugeEchart(id, MachineInfo_UID, result[i].FirstDashBoardTarget / 100, result[i].SecondDashBoardTarget / 100, (result[i].OEEValue * 100).toFixed(2) + "%");
                            }
                            if (result[i].IsTotalStationStatic == "YES") {
                                $("#id_Station_FirstYield").text("Quality(OK): " + result[i].FirstYield);
                                $("#id_Station_PerformanceRate").text("Performance: " + result[i].PerformanceRate);
                                $("#id_Station_AvailableRate").text("Availability: " + result[i].AvailableRate);
                                var oeestationValue = parseFloat(result[i].OEEValue);
                                if (oeestationValue < 50) {
                                    $("#id_Station_OeeValue").attr("style", "text-align:center; background-color :#c23531;margin-left:1%");
                                } else if (oeestationValue < 75) {
                                    $("#id_Station_OeeValue").attr("style", "text-align:center; background-color :#63869e;margin-left:1%");
                                } else {
                                    $("#id_Station_OeeValue").attr("style", "text-align:center; background-color :#91c7ae;margin-left:1%");
                                };

                                var stationName = $('#stationName option:selected').text();
                                if (stationName == "" || stationName == null) {
                                    stationName = "Station OeeValue:";
                                }
                                $("#id_Station_OeeValue").text(stationName + "：" + result[i].OEEValue);
                            }
                            if (result[i].IsTotalStationStatic == "PAGE") {
                                $("#page").paging({
                                    pageNo: $(".current").text(),
                                    totalPage: result[i].TotalPage,
                                    totalSize: result[i].TotalCount,
                                    //totalPage: 5,
                                    //totalSize: 45,
                                    callback: function () {
                                        ShowAllStationOEE($(".current").text())
                                    }
                                })
                            }
                        }

                        if (exportDTCode) {
                            $("#btn_DTCode_Exprot").css("display", "");
                        } else {
                            $("#btn_DTCode_Exprot").css("display", "none");
                        }

                        if (exportDFCode) {
                            $("#btn_DFCode_Exprot").css("display", "");
                        } else {
                            $("#btn_DFCode_Exprot").css("display", "none");
                        }
                    },
                    error: function () {
                        console.log('error');
                    }
                });
            }
