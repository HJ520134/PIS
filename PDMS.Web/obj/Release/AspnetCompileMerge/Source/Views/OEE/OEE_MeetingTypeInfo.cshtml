
@model PDMS.Model.ViewModels.Fixture_DefectCodeVM

@{
    ViewBag.OEE_AddMachine = "新增会议基本信息";
    ViewBag.OEE_UpdateMachine = "编辑会议基本信息";
}
<!-- Main content -->
@*<section class="content-header portal-content-header">
        <h1>@T("OEE.HomeMachineMaintence")</h1>
    </section>*@
<section class="content portal-content">

    <!--次標題 與 搜尋-->
    <div class="row">
        <!--次標題與Search keyword-->
        <div class="col-md-12 col-lg-8">
            <h4 class="margin-td-5" id="js_search_keywords"></h4>
        </div><!-- /col-次標題與Search keyword -->
        <div class="col-md-12 search-field col-lg-12">
