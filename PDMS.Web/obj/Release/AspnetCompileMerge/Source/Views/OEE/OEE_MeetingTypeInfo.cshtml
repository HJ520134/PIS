                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="js_edit_funplant_query">@T("QA.Functionfactory")</label>
                                    <div class="col-sm-8">
                                        <select class="selectpicker form-control input-sm" id="js_edit_funplant_query" name="FunPlant_Organization_UID" data-live-search="true"></select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="js_edit_MeetingType_ID">会议ID</label>
                                    <div class="col-sm-8">
