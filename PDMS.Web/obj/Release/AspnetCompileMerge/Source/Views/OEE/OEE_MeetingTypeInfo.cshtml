                @using (Html.BeginForm("AddOEE_MeetingTypeInfo", "OEE", FormMethod.Post, new { id = "js_form_OEE_Machine_edit" }))
                {
                <div class="modal-body form-horizontal">
                    <div class="row">
                        <form id="js_edit_form_query" data-need-validate="true">
                            <input type="hidden" name="MeetingType_UID" id="js_hidden_MeetingType_UID" value="0" />
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="js_edit_factory_query">@T("QA.Plantarea")</label>
                                    <div class="col-sm-8">
                                        <select class="selectpicker form-control input-sm" id="js_edit_factory_query" name="Plant_Organization_UID" data-live-search="true">
                                            <option></option>
                                            @foreach (var item in Model.Plants)
                                                {
                                                <option value=@item.Plant_OrganizationUID>@item.Plant</option>
