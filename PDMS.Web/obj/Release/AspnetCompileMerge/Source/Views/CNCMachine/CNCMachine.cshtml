}
@section ViewScripts{
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
    <script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
    <script type="text/javascript">
  
    $('#js_btn_clear').click(function () {
        PDMS.Utility.Criteria.Clear();
        $('#js_search_modal').find('select').val('');
    });

    $('#js_search_modal').on('hidden.bs.modal', function (e) {
        $('#js_search_modal').find('input').val('');
        $('#js_search_modal').find('select').val('');
    })
    $('#btn_import_fl').on('click', function () {
        $('#js_importExcel_modal').modal('show');
    });

    $('#btn_clear_Update').on('click', function () {
        $('#js_importExcel_modal').modal('hide');
    });



    $('#btn_clear_download').on('click', function () {
        $('#js_download_modal').modal('hide');
    });

    $(function () {
        var StorageCheckSetting = (function () {
            var urls = {
                //根据厂区取得OP类型
                getCurrentOPType: '@Url.Action("GetCurrentOPType", "Fixture")',
                //根据OP类型取得功能厂
                getFunPlantByOPTypes: '@Url.Action("GetFunPlantByOPTypes", "Fixture")',
                //画面初始化加载
                queryStorageChecks: '@Url.Action("QueryCNCMachineDTOs", "CNCMachine")',
                //获取专案
                getProjectList:'@Url.Action("GetSystemProjectDTO", "CNCMachine")',

                queryStorageCheckByUid:'@Url.Action("QueryCNCMachineDTOByUid", "CNCMachine")',
                //删除未审核盘点数据
                deleteStorageCheck: '@Url.Action("DeleteCNCMachine", "CNCMachine")',
                //导出excel
                doExportStorageCheckReprot: '@Url.Action("DoExportCNCMachineReprot", "CNCMachine")',
                doAllExportStorageCheckReprot: '@Url.Action("DoAllExportCNCMachineReprot", "CNCMachine")'

            };
            //#region 定义字段列
            var columns = [
                {
                    createdCell: function (td, cellData, rowData, row, col) {

                        $(td).html('<input type="checkbox" name="cktrans" class="js-checkbox-item" value="' + rowData.CNCMachineUID + '">')
                                        .addClass('table-col-checkbox');

                    },
                    className: "text-center"
                },{
                    data: null,
                    className: "table-col-seq"
                }, {
                    createdCell: function (td, cellData, rowData, row, col) {
                        var buttonEdit = '<button type="button" class="btn btn-primary btn-xs js-grid-edit"  data-id="' + rowData.CNCMachineUID + '">编辑</button>';

                        var buttonDelete = '<button type="button" class="btn btn-primary btn-xs js-grid-delete"   data-id="' + rowData.CNCMachineUID + '">删除</button>';
                        var result = '<button class="btn btn-primary btn-xs" rel="action-popover">' +
                                    '<i class="fa fa-reorder"></i>' +
                                    '</button>' +
                                    '<div class="hidden popover-content">' +
                                        '{0}' +
                                        '{1}' +
                                    '</div>';
                            result = result.replace('{0}', buttonEdit);
                            result = result.replace('{1}', buttonDelete);


                        $(td).html(result);
                    },
                    className: "text-center"
                }, {
                    data: "Plant_Organization_Name",
                    className: "min-col-xs"
                }, {
                    data: "BG_Organization_Name",
                    className: "min-col-xs"
                }, {
                    data: "FunPlant_Organization_Name",
                    className: "min-col-xs"
                }, {
                    data: "ProjectName",
                    className: "min-col-xs"
                }, {
                    data: "Equipment",
                    className: "min-col-xs"
                }, {
                    data: "Machine_ID",
                    className: "min-col-xs"
                }, {
                    data: "Machine_Name",
                    className: "min-col-xs"
                }, {
                    data: "Is_Enable",
                    render: function (data, type, full, meta) {
                        return data ? "Y" : "N";
                    },
                    className: "min-col-xs"
                },
                {
                    data: "Modifyer",
                    className: "min-col-xs"
                }, {

                    data: null,
                createdCell:
            function (td, cellData, rowData, row, col) {
                $(td).html(new Date(rowData.Modified_Date).format("yyyy-MM-dd hh:mm:ss"));},
            className: "min-col-xs"
                }];

            //#endregion 定义字段列
            Date.prototype.format = function(format) {
                var date = {
                    "M+": this.getMonth() + 1,
                    "d+": this.getDate(),
                    "h+": this.getHours(),
                    "m+": this.getMinutes(),
                    "s+": this.getSeconds(),
                    "q+": Math.floor((this.getMonth() + 3) / 3),
                    "S+": this.getMilliseconds()
                };
                if (/(y+)/i.test(format)) {
                    format = format.replace(RegExp.$1, (this.getFullYear() + '').substr(4 - RegExp.$1.length));
                }
                for (var k in date) {
                    if (new RegExp("(" + k + ")").test(format)) {
                        format = format.replace(RegExp.$1, RegExp.$1.length == 1
                               ? date[k] : ("00" + date[k]).substr(("" + date[k]).length));
                    }
                }
                return format;
            }
            var _getParams = function () {
                PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");

                return $('#js_form_query').serialize().replace(/\+/g, " ");
            };

            var _queryStorageChecks = function (firstLoad, buildCriteria) {

                var config = {
                    pageId: "#page",
                    tableId: "#js_Bom_datatable",
                    remoteUrl: urls.queryStorageChecks,
                    searchParams: _getParams(),
                    tableOptions: {
                        columns: columns
                    }
                };
                if (!firstLoad) {
                    $('#page').page('destroy');
                }
                if (buildCriteria) {
                    PDMS.Utility.Criteria.Build();
                }
                PDMS.Utility.Pages.Set(config);
                $('table thead tr').find('th').removeClass('text-right');
            };
            return {
                urls: urls,
                Init: function () {
                    PDMS.Utility.Criteria.Init();
                    _queryStorageChecks(true, false);
                },
                queryStorageChecks: function (buildCriteria) {
                    if (!buildCriteria) {
                        buildCriteria = false;
                    }
                    _queryStorageChecks(false, buildCriteria);
                }
            }
        })();

        StorageCheckSetting.Init();


        //查询按钮
        $('#js_btn_inner_query').click(function () {
            if ($('#js_form_query').valid()) {
                StorageCheckSetting.queryStorageChecks(true);
                $('#js_search_modal').modal('hide');
            }
        });

        //添加按钮
        $('#js_btn_query').click(function () {
            $('#js_select_factory_search').trigger('change');
        })

        $('#js_select_factory_search').change(function () {
            GetOPTypesearch();
        });
        function GetOPTypesearch() {
            // debugger;
            var oporgid = $('#js_select_factory_search option:selected').val();
            url = StorageCheckSetting.urls.getCurrentOPType;
            $('#js_select_optype_search').html('<option></option>');
            $('#js_select_optype_search').selectpicker('refresh');
            $('#js_select_funplant_search').html('<option></option>');
            $('#js_select_funplant_search').selectpicker('refresh');
            $('#js_select_Project_search').html('<option></option>');
            $('#js_select_Project_search').selectpicker('refresh');

            if (oporgid != 0) {
                //设置OP
                $.post(url, { plant_OrganizationUID: oporgid }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        if ($('#js_select_optype_search').find('option[value=' + data[i].Organization_UID + ']').length == 0) {
                            if(@Model.OptypeID!=0)
                            {
                                if(@Model.OptypeID==data[i].Organization_UID )
                                {
                                    $('#js_select_optype_search').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');

                                }
                            }else
                            {
                                $('#js_select_optype_search').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');

                            }
                        }
                    }

                    $('#js_select_optype_search').selectpicker('refresh');
                });
                //url = StorageCheckSetting.urls.getOEE_DownTypeDTOs;
                //$.post(url, { Plant_Organization_UID: oporgid }, function (data) {
                //    for (var i = 0; i < data.length; i++) {
                //        $('#js_select_DownTimeType_search').append('<option value="' + data[i].OEE_DownTimeType_UID + '">' + data[i].Type_Name + '</option>');
                //    }

                //    $('#js_select_DownTimeType_search').selectpicker('refresh');
                //});

            }

            //设专案加载
            url=StorageCheckSetting.urls.getProjectList;
            var oporgid = $('#js_select_factory_search option:selected').val();
            $.post(url, { Plant_Organization_UID: oporgid ,BG_Organization_UID: 0}, function (data) {
                debugger;
                for (var i = 0; i < data.length; i++) {
                    $('#js_select_Project_search').append('<option value="' + data[i].Project_UID + '">' + data[i].Project_Name + '</option>');
                }
                $('#js_select_Project_search').selectpicker('refresh');
            });
        }
        //OP类型变更  start
        $('#js_select_optype_search').change(function () {
            var url = StorageCheckSetting.urls.getFunPlantByOPTypes;
            $('#js_select_funplant_search').html('<option></option>');
            $('#js_select_funplant_search').selectpicker('refresh');
            $('#js_select_Project_search').html('<option></option>');
            $('#js_select_Project_search').selectpicker('refresh');

           // if ($('#js_select_funplant_search option:selected').text() != "") {
                //设置功能厂
                $.post(url, { Optype: $('#js_select_optype_search').val() }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        if(@Model.FunPlantID!=0)
                        {
                            if(@Model.FunPlantID==data[i].FunPlant_OrganizationUID )
                            {
                                $('#js_select_funplant_search').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');

                            }
                        }else
                        {
                            $('#js_select_funplant_search').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');

                        }
                    }
                    $('#js_select_funplant_search').selectpicker('refresh');
                });

           // }

            //设专案加载
