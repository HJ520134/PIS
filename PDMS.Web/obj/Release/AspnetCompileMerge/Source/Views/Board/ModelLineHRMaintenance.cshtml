<!-- Main content -->
<section class="content portal-content">

    <!--次標題 與 搜尋-->
    <div class="row">
        <!--次標題與Search keyword-->
        <div class="col-md-12 col-lg-8">
            <h4 class="margin-td-5" id="js_search_keywords"></h4>
        </div><!-- /col-次標題與Search keyword -->
        <!--col-右方搜尋與功能按鈕列-->
        <div class="col-md-12 search-field col-lg-4">
            @*<button type="button" class="fa fa-upload btn btn-primary" id="excel_input_btn">@T("QA.Importtheupdate")</button>*@
            <a href="~/ExcelTemplate/ModelLine_HR.xlsx" class="fa fa-download btn btn-primary" target="_blank"> 模板下载</a>
            <button type="button" class="fa fa-upload btn btn-primary" id="btn_import_fl"> 上传</button>
            <a class="fa fa-plus btn btn-primary" role="button" data-toggle="modal" data-target="#js_edit_modal" id="js_btn_add_ModelLineHR"> @T("Common.AddNew")</a>
            @*<a id="js_btn_export" class="fa fa-download btn btn-primary" role="button"> @T("Common.Export")</a>*@
        </div><!-- /col-右方搜尋與功能按鈕列-->
    </div><!--/次標題 與 搜尋-->
    <hr class="hr-custom">

    <!--row Search Keyword collapse-->
    <div class="row">
        <div id="js_criteria_alert_cnt">
        </div>
    </div> <!--/row Search Keyword collapse-->
    <!--內容 表格列-->
    <div class="row">
        <!--表格-->
        <div class="col-md-12 table-container">
            <table class="table table-striped table-hover table-condensed nowrap" id="js_ModelLineHR_datatable">
                <thead>
                    <tr>
                        <th class="table-col-checkbox nosort">
                            <input type="checkbox" class="js-checkbox-all" />
                        </th>
                        <th class="table-col-action nosort">@T("Common.Action")</th>
                        <th class="table-col-seq nosort">@T("Common.Seq")</th>
                        <th>工站</th>
                        <th>总人力</th>
                        <th>应到</th>
                        <th>实到</th>
                        <th>休息</th>
                        <th>事假</th>
                        <th>病假</th>
                        <th>旷工</th>
                        <th>创建日期</th>
                        <th>修改日期</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th class="table-col-checkbox nosort"></th>
                        <th class="table-col-action nosort">@T("Common.Action")</th>
                        <th class="table-col-seq nosort">@T("Common.Seq")</th>
                        <th>工站</th>
                        <th>总人力</th>
                        <th>应到</th>
                        <th>实到</th>
                        <th>休息</th>
                        <th>事假</th>
                        <th>病假</th>
                        <th>旷工</th>
                        <th>创建日期</th>
                        <th>修改日期</th>
                    </tr>
                </tfoot>
            </table>
            <input type="hidden" name="System_edit_id" id="js_hidden_edit_id" value="0" />
            <div id="page" class="row"></div>

        </div><!--/表格-->
    </div><!-- / 內容 表格列 -->

</section><!-- /.content -->

@section ViewModals{
    <div class="modal fade" id="js_edit_modal" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Maintain ModelLineHR</h4>
                </div>

                @using (Html.BeginForm("AddOrEditModelLineHR", "Board", FormMethod.Post, new { id = "js_form_ModelLineHR_edit" }))
                {
                    <div class="modal-body form-horizontal">
                        <div class="row">
                            <input type="hidden" name="ModelLineHR_UID" id="js_hidden_ModelLineHR_UID" value="0" />

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_Station">工站</label>
                                    <div class="col-sm-7">
                                        <input type="text" class="form-control input-sm required" id="js_input_Station" name="Station" placeholder="工站"
                                               required data-msg-required="请输入[工站]."
                                               data-rule-maxlength="50" data-msg-maxlength="工站代不能超过 {0} 个字符.">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_Total">总人力</label>
                                    <div class="col-sm-7">
                                        <input type="number" min="0"  class="form-control input-sm" id="js_input_Total" name="Total">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_ShouldCome">应到</label>
                                    <div class="col-sm-7">
                                        <input type="number" min="0"  class="form-control input-sm" id="js_input_ShouldCome" name="ShouldCome">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_ActualCome">实到</label>
                                    <div class="col-sm-7">
                                        <input type="number" min="0"  class="form-control input-sm" id="js_input_ActualCome" name="ActualCome">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_VacationLeave">休息</label>
                                    <div class="col-sm-7">
                                        <input type="number" min="0"  class="form-control input-sm" id="js_input_VacationLeave" name="VacationLeave">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_PersonalLeave">事假</label>
                                    <div class="col-sm-7">
                                        <input type="number" min="0"  class="form-control input-sm" id="js_input_PersonalLeave" name="PersonalLeave">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_SickLeave">病假</label>
                                    <div class="col-sm-7">
                                        <input type="number" min="0"  class="form-control input-sm" id="js_input_SickLeave" name="SickLeave">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-sm-5 control-label" for="js_input_AbsentLeave">旷工</label>
                                    <div class="col-sm-7">
                                        <input type="number" min="0"  class="form-control input-sm" id="js_input_AbsentLeave" name="AbsentLeave">
                                    </div>
                                </div>
                            </div>
                            <!--jquery validata error container-->
                            <div class="col-xs-12"><ul class="list-group validate-error"></ul></div>

                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary btn-sm" id="js_btn_save_edit_ModelLineHR"><i class="fa fa-save"></i> 保存</button>
                        <button type="button" class="btn btn-primary btn-sm" id="js_btn_save_new_ModelLineHR"><i class="fa fa-save"></i> 保存</button>
                        <button class="btn btn-primary btn-sm" data-dismiss="modal"><i class="fa fa-times"></i> 取消</button>
                    </div>
                }
            </div>
        </div>
    </div>
    <div class="modal fade" id="js_importExcel_modal" role="dialog">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Model Line 人力数据导入</h4>
                </div>
                @using (Html.BeginForm("ImportModelLineHR", "Board", FormMethod.Post, new { enctype = "multipart/form-data", id = "js_form_excel_upload" }))
                {
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-sm-2">
                                <label class="control-label" for="js_s_input_import">选择Excel档</label>
                            </div>
                            <div class="col-sm-8">
                                <input type="file" class="form-control" id="js_s_input_import" name="uploadName" placeholder="选择文件"
                                       required data-msg-required="请选择要上传的Excel档!" accept=".xls,.xlsx" />
                            </div>
                        </div>
                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary btn-sm" id="btn_clear_Update"><i class="fa fa-times"></i>取消</button>
                        <button type="button" class="btn btn-primary btn-sm" id="btn_excel_upload"><i class="fa fa-save"></i>提交</button>

                    </div>
                }
            </div>
        </div>
    </div>
}

@section ViewScripts{
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
    <script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
    <script type="text/javascript">
        $('#btn_import_fl').on('click', function () {
            $('#js_importExcel_modal').modal('show');
        });
        $('#btn_clear_Update').on('click', function () {
            $('#js_importExcel_modal').modal('hide');
        });
        $(function () {
            var ModelLineHRMaintenance = (function () {
                var needBuildCriteria = false;
                var urls =
                    {
                        EditModelLineHR: '@Url.Action("EditModelLineHR", "Board")',
                        AddModelLineHR: '@Url.Action("AddModelLineHR", "Board")',
                        queryModelLineHRs: '@Url.Action("QueryModelLineHRs", "Board")',
                        queryModelLineHR: '@Url.Action("QueryModelLineHR", "Board")',
                        deleteModelLineHR: '@Url.Action("DeleteModelLineHR", "Board")',
                        GetMaxEnddate4ModelLineHR: '@Url.Action("GetMaxEnddate4ModelLineHR", "Board")',
                        exportModelLineHRs: '@Url.Action("DoExportModelLineHR", "Board")',
                        exportModelLineHRsByQuery: '@Url.Action("DoExportModelLineHRByQuery", "Board")',
                        //根据厂区取得OP类型
                        getCurrentOPType: '@Url.Action("GetCurrentOPType", "Board")',
                        //根据OP类型取得功能厂
                        getFunPlantByOPTypes: '@Url.Action("GetFunPlantByOPTypes", "Board")',
                        getProjectByOPType: '@Url.Action("QueryProjects", "Equipmentmaintenance")',
                        getProcess_InfoList: '@Url.Action("GetProcess_InfoList", "Board")',
                        downloadExcel: '@Url.Action("ModelLineHR", "Board")',
                    };
                var columns = [
                    {
                        createdCell: function (td, cellData, rowData, row, col) {
                            $(td).html('<input type="checkbox" name="cktrans" class="js-checkbox-item" value="' + rowData.ModelLineHR_UID + '">')
                                 .addClass('table-col-checkbox');
                        },
                        className: "text-center"
                    },
                    {
                        createdCell: function (td, cellData, rowData, row, col) {
                            var buttonHtml = '';
                            if (rowData.ModelLineHR_UID > 0) {
                                buttonHtml = '<button class="btn btn-primary btn-xs" rel="action-popover"><i class="fa fa-reorder"></i></button>' +
                                        '<div class="hidden popover-content">' +
                                            '<button type="button" class="btn btn-primary btn-xs js-grid-edit" data-id="' + rowData.ModelLineHR_UID + '">编辑</button>' +
                                            '<button type="button" class="btn btn-primary btn-xs js-grid-delete" data-id="' + rowData.ModelLineHR_UID + '" data-modelLineHRStation = "' + rowData.Station + '">删除</button>' +
                                        '</div>';
                            }
                            var resultHtml = buttonHtml;
                            $(td).html(resultHtml);
                        },
                        className: "text-center"
                    }, {
                        data: null,
                        className: "table-col-seq"
                    }, {
                        data: "Station",
                        className: "min-col-xs"
                    }, {
                        data: "Total",
                        className: "min-col-xs"
                    }, {
                        data: "ShouldCome",
                        className: "min-col-xs"
                    }, {
                        data: "ActualCome",
                        className: "min-col-xs"
                    }, {
                        data: "VacationLeave",
                        className: "min-col-xs"
                    }, {
                        data: "PersonalLeave",
                        className: "min-col-xs"
                    }, {
                        data: "SickLeave",
                        className: "min-col-xs"
                    }, {
                        data: "AbsentLeave",
                        className: "min-col-xs"
                    }, {
                        data: "Created_Date",
                        className: "min-col-xs"
                    }, {
                        data: "Modified_Date",
                        className: "min-col-xs"
                    }
                ];

                var _getParams = function () {
                    if (needBuildCriteria) {
                        PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                        return $('#js_form_query').serialize().replace(/\+/g, " ");
                    }
                    else {
                        return null;
                    }
                };

                var _queryModelLineHRs = function (firstLoad) {

                    var config = {
                        pageId: "#page",
                        tableId: "#js_ModelLineHR_datatable",
                        remoteUrl: urls.queryModelLineHRs,
                        //searchParams: _getParams(),
                        tableOptions: {
                            scrollX: true,
                            scrollCollapse: false,
                            autoWidth: true,
                            columns: columns
                        }
                    };
                    
                    if (!firstLoad) {
                        $('#page').page('destroy');
                    }

                    //if (needBuildCriteria) {
                    //    PDMS.Utility.Criteria.Build();
                    //}

                    PDMS.Utility.Pages.Set(config);
                };

                return {
                    urls: urls,
                    Init: function () {
                        //页面初始化加载查询时，初始化Criteria
                        PDMS.Utility.Criteria.Init();
                        _queryModelLineHRs(true);
                    },
                    queryModelLineHRs: function (isSearchBtn) {
                        needBuildCriteria = (isSearchBtn === true ? true : needBuildCriteria);
                        _queryModelLineHRs(false);
                    }
                }

            })();
            ModelLineHRMaintenance.Init();

            $('#js_btn_export').click(function () {
                var $selectList = $('#js_ModelLineHR_datatable').find('.js-checkbox-item:checked');
                var len = $selectList.length;
                if (len == 0) {
                    //全部导出
                    var url = ModelLineHRMaintenance.urls.exportModelLineHRsByQuery;
                    //没有查询条件的情况，从查询页面获取
                    if (PDMS.Utility.Settings.Pages.remote.params == null) {
                        PDMS.Utility.Settings.Pages.remote.params = $('#js_form_query').serialize().replace(/\+/g, " ");
                    }
                    url += '?' + PDMS.Utility.Settings.Pages.remote.params;
                    window.location.href = url;
                } else {
                    var uids = $.map($selectList, function (row) {
                        return row.value;
                    });
                    var url = ModelLineHRMaintenance.urls.exportModelLineHRs;
                    url += "?uids=" + uids.toString();
                    window.location.href = url;
                }
            });

            //extra configuration of validate in add/edit model, others via data-attribute
            $('#js_form_ModelLineHR_edit').validate({
                errorContainer: $('ul.validate-error'),
                errorLabelContainer: $('#js_edit_modal ul.validate-error'),
                wrapper: 'li'
            });

            //clear query conditions in search modal
            $('#js_btn_clear').click(function () {

                PDMS.Utility.Criteria.Clear(function () {
                    $('#js_s_radio_valid').prop('checked', true);
                    $("#js_s_input_reference_date").val(moment().format('YYYY-MM-DD'));
                });
            });

            //do clean up in edit modal show/hide
            $('#js_edit_modal').on('show.bs.modal', function (event) {
                var button = $(event.relatedTarget)
                var modal = $(this)
                var title = modal.find('.modal-title');
                if (button.hasClass('js-grid-edit')) {
                    //编辑
                    title.text("编辑");
                    $('#js_btn_save_new_ModelLineHR').hide();
                    $('#js_btn_save_edit_ModelLineHR').show();
                    $("#js_hidden_edit_id").val(0);

                    var uid = button.attr("data-id");
                    //调api 获取信息，填充到编辑画面
                    $.get(ModelLineHRMaintenance.urls.queryModelLineHR, { uid: uid }, function (data) {
                      
                        $('#js_hidden_ModelLineHR_UID').val(data.ModelLineHR_UID);
                        $("#js_input_Station").val(data.Station);
                        $("#js_input_Total").val(data.Total);
                        $("#js_input_ShouldCome").val(data.ShouldCome);
                        $("#js_input_ActualCome").val(data.ActualCome);
                        $("#js_input_VacationLeave").val(data.VacationLeave);
                        $("#js_input_PersonalLeave").val(data.PersonalLeave);
                        $("#js_input_SickLeave").val(data.SickLeave);
                        $("#js_input_AbsentLeave").val(data.AbsentLeave);
                        //$('#js_select_factory_add').val(data.Plant_Organization_UID);
                        //$('#js_select_factory_add').trigger('change');

                        //$('#js_select_optype_add').val(data.BG_Organization_UID);
                        //$('#js_select_optype_add').trigger('change');

                        //$('#js_select_funplant_add').val(data.FunPlant_Organization_UID);
                        //$('#js_select_funplant_add').trigger('change');



                        //$('#js_select_factory_add').selectpicker('val', data.Plant_Organization_UID);
                        //$('#js_select_factory_add').selectpicker('refresh');
                        //$('#js_select_factory_add').trigger('change');
                        //$('#js_select_optype_add').selectpicker('val', data.BG_Organization_UID);
                        //$('#js_select_optype_add').selectpicker('refresh');
                        //if (data.BG_Organization_UID != 0) {
                        //    $('#js_select_optype_add').trigger('change');
                        //}
                        //$('#js_select_funplant_add').selectpicker('val', data.FunPlant_Organization_UID);
                        //$('#js_select_funplant_add').selectpicker('refresh');
                        //$("#js_select_factory_add").attr('disabled', 'disabled');
                        //$("#js_select_optype_add").attr('disabled', 'disabled');
                        //$("#js_select_funplant_add").attr('disabled', 'disabled');
                        //$('#js_select_project_add').selectpicker('val', data.Project_UID);
                        //$('#js_select_project_add').selectpicker('refresh');

                        //$('#js_select_process_add').selectpicker('val', data.Process_Info_UID);
                        //$('#js_select_process_add').selectpicker('refresh');
                        //$('#js_select_modelLineHRId_add').val(data.ModelLineHR_ID);
                        //$('#js_input_ModelLineHR_Name_add').val(data.ModelLineHR_Name);
                        //$('#js_input_ModelLineHR_Desc_add').val(data.ModelLineHR_Desc);
                        ////$('#js_select_project_add').val(data.Project_UID);
                        ////$('#js_select_process_add').val(data.Process_Info_UID);
                        //$('#js_e_select_enable_flag').val(data.Is_Enable.toString());

                    });


                } else if (button.attr('id') == 'js_btn_add_ModelLineHR') {
                    //新增
                    title.text("新增工站");

                    $("#js_select_factory_add").removeAttr("disabled");
                    $("#js_select_optype_add").removeAttr("disabled");
                    $("#js_select_funplant_add").removeAttr("disabled");

                    $('#js_select_factory_add').trigger('change');
                    $('#js_select_optype_add').trigger('change');

                    $('#js_select_modelLineHRId_add').val('');
                    $('#js_input_ModelLineHR_Name_add').val('');
                    $('#js_input_ModelLineHR_Desc_add').val('');
                    $('#js_e_select_enable_flag').val('false');
                    //$('#js_select_project_add').val('');

                    //$('#js_input_ModelLineHR').data('original', '');
                    //$('#js_input_end_date').attr("disabled", false);
                    $('#js_btn_save_new_ModelLineHR').show();
                    $('#js_btn_save_edit_ModelLineHR').hide();
                }
            })
            $('#js_edit_modal').on('hidden.bs.modal', function (e) {

                $('#js_edit_modal').find('input').val('');
                $('.list-group.validate-error').empty();
            });

            //edit button in grid
            $('body').on('click', '.js-grid-edit', function () {




                var $sender = $(this);
                if ($sender.hasClass('disabled')) {
                    PDMS.Utility.MessageBox.error("ModelLineHR already expired, can't edit!");
                    return;
                }

                $("#js_hidden_edit_id").val(1);
                var uid = $(this).attr('data-id'),
                    url = ModelLineHRMaintenance.urls.queryModelLineHR;
                $.post(url, { uid: uid }, function (data) {
                    //隐藏字段
                    $('#js_hidden_ModelLineHR_uid').val(uid);
                    $('#js_hidden_Plant_Organization_uid').val(data.Plant_Organization_UID);
                    $('#js_hidden_BG_Organization_uid').val(data.BG_Organization_UID);
                    $('#js_hidden_FunPlant_Organization_uid').val(data.FunPlant_Organization_UID);

                    //显示字段
                    $('#js_input_ModelLineHR_Name_add').val(data.ModelLineHR_Name);
                    $('#js_input_BuildingName').val(data.Building_Name);
                    $('#js_input_FloorName').val(data.Floor_Name);
                });

                $('#js_edit_modal').modal('show', $(this));

            });

            //delete button in grid
            $('body').on('click', '.js-grid-delete', function () {
                var uid = $(this).attr('data-id');
                var station = $(this).attr('data-modelLineHRStation');
                PDMS.Utility.MessageBox.confirm(
                    "你确定要删除工站为[" + station + "]的人力数据吗?"
                    , function () {
                        var url = ModelLineHRMaintenance.urls.deleteModelLineHR;
                        $.post(url, { uid: uid }, function (data) {

                            if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("删除成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error("Error:" + data);
                            }
                        });
                    });
            });

            firstSearch = true;
            $('#bt_search').click(function () {
                if (firstSearch) {
                    $('#js_select_factory_query').trigger('change');
                    firstSearch = false;
                }
            })

            //查询界面厂区是OP类型变化
            $('#js_select_factory_query').change(function () {
                GetOPTypes();
                refreshSearchProcessSelect();
            })
            $('#js_select_factory_add').change(function () {
                var oporgid = $('#js_select_factory_add option:selected').val();
                if (oporgid != 0) {
                    //op类型
                    url = ModelLineHRMaintenance.urls.getCurrentOPType;
                    $('#js_select_optype_add').empty();
                    $.post(url, { plant_OrganizationUID: oporgid }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#js_select_optype_add').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');
                            $('#js_select_optype_add').selectpicker('refresh');
                        }
                    });
                    $('#js_select_optype_add').trigger('change');
                }
            })

            //编辑时optype 改变时间
            $('#js_select_optype_add').change(function () {
                if ($('#js_select_optype_add option:selected').text() != "") {
                    //功能厂
                    refreshFuncPlantSelectAdd();
                    //专案
                    refreshProjectSelectAdd();
                    //制程
                    refreshProcessSelectAdd();
                } else {
                    $('#js_select_funplant_add').empty();
                    $('#js_select_funplant_add').html('<option></option>');
                    $('#js_select_project_add').empty();
                    $('#js_select_process_add').empty();
                }
            });

            //新建中功能厂列表刷新
            function refreshFuncPlantSelectAdd() {
                $('#js_select_funplant_add').empty();
                $('#js_select_funplant_add').html('<option></option>');
                var getFuncPlantUrl = ModelLineHRMaintenance.urls.getFunPlantByOPTypes;
                $.post(getFuncPlantUrl, { Optype: $('#js_select_optype_add').val() }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_funplant_add').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');
                        $('#js_select_funplant_add').selectpicker('refresh');
                    }
                });
            }

            //新建中专案列表刷新
            function refreshProjectSelectAdd() {
                $('#js_select_project_add').empty();
                $('#js_select_project_add').html('<option></option>');
                var getProjectByOPTypeUrl = ModelLineHRMaintenance.urls.getProjectByOPType;
                $.get(getProjectByOPTypeUrl, { oporgid: $('#js_select_optype_add').val() }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_project_add').append('<option value="' + data[i].Project_UID + '">' + data[i].Project_Name + '</option>');
                        $('#js_select_project_add').selectpicker('refresh');
                    }
                })
            }

            //新建中制程列表刷新
            function refreshProcessSelectAdd() {
                $('#js_select_process_add').empty();
                $('#js_select_process_add').html('<option></option>');
                url = ModelLineHRMaintenance.urls.getProcess_InfoList;
                var funcPlantUid = $('#js_select_funplant_add').val();
                if (funcPlantUid == "" || funcPlantUid == undefined) {
                    funcPlantUid = 0;
                }
                $.post(url, { Plant_Organization_UID: $('#js_select_factory_add').val(), BG_Organization_UID: $('#js_select_optype_add').val(), FunPlant_Organization_UID: funcPlantUid }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_process_add').append('<option value="' + data[i].Process_Info_UID + '">' + data[i].Process_ID + '_' + data[i].Process_Name + '</option>');
                        $('#js_select_process_add').selectpicker('refresh');
                    }
                });
            }

            function GetOPTypes() {
                var oporgid = $('#js_select_factory_query option:selected').val();
                url = ModelLineHRMaintenance.urls.getCurrentOPType;
                $('#js_select_optype_query').empty();
                $('#js_select_optype_query').html('<option></option>');

                $('#js_select_funplant_query').empty();
                $('#js_select_funplant_query').html('<option></option>');
                if (oporgid != 0) {
                    $.post(url, { plant_OrganizationUID: oporgid }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            if ($('#js_select_optype_query').find('option[value=' + data[i].Organization_UID + ']').length == 0) {
                                $('#js_select_optype_query').append('<option value="' + data[i].Organization_UID + '">' + data[i].OP_TYPES + '</option>');
                                $('#js_select_optype_query').selectpicker('refresh');
                            }
                        }
                    });
                }
            }

            //OP类型变更  start
            $('#js_select_optype_query').change(function () {
                if ($('#js_select_optype_query option:selected').text() != "") {
                    refreshSearchFuncPlantSelect();
                    refreshSearchProjectSelect();
                    refreshSearchProcessSelect();
                } else {
                    $('#js_select_funplant_query').empty();
                    $('#js_select_funplant_query').html('<option></option>');
                    $('#js_select_project_query').empty();
                    $('#js_select_project_query').html('<option></option>');
                    $('#js_select_process_query').empty();
                    $('#js_select_process_query').html('<option></option>');
                }
            })

            //功能厂change
            $('#js_select_funplant_query').change(function () {
                if ($('#js_select_funplant_query option:selected').text() != "") {
                    refreshSearchProcessSelect();
                } else {
                    $('#js_select_process_query').empty();
                    $('#js_select_process_query').html('<option></option>');
                }

            });

            //刷新功能厂下拉框
            function refreshSearchFuncPlantSelect() {
                $('#js_select_funplant_query').empty();
                $('#js_select_funplant_query').html('<option></option>');
                var url = ModelLineHRMaintenance.urls.getFunPlantByOPTypes;
                $.post(url, { Optype: $('#js_select_optype_query').val() }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_funplant_query').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');
                        $('#js_select_funplant_query').selectpicker('refresh');
                    }
                });
            }

            //刷新搜索中专案下拉框
            function refreshSearchProjectSelect() {
                $('#js_select_project_query').empty();
                $('#js_select_project_query').html('<option></option>');
                var getProjectByOPTypeUrl = ModelLineHRMaintenance.urls.getProjectByOPType;
                $.get(getProjectByOPTypeUrl, { oporgid: $('#js_select_optype_query').val() }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_project_query').append('<option value="' + data[i].Project_UID + '">' + data[i].Project_Name + '</option>');
                        $('#js_select_project_query').selectpicker('refresh');
                    }
                });
            }

            //刷新搜索中制程下拉框
            function refreshSearchProcessSelect() {
                $('#js_select_process_query').empty();
                $('#js_select_process_query').html('<option></option>');
                url = ModelLineHRMaintenance.urls.getProcess_InfoList;
                var funplantUid = $('#js_select_funplant_query').val();
                if (funplantUid == "" || funplantUid == undefined) {
                    funplantUid = 0;
                }
                $.post(url, { Plant_Organization_UID: $('#js_select_factory_query').val(), BG_Organization_UID: $('#js_select_optype_query').val(), FunPlant_Organization_UID: funplantUid }, function (data) {

                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_process_query').append('<option value="' + data[i].Process_Info_UID + '">' + data[i].Process_ID + '_' + data[i].Process_Name + '</option>');
                        $('#js_select_process_query').selectpicker('refresh');
                    }
                });
            }


            $('#js_select_funplant_add').change(function () {
                refreshProcessSelectAdd();
            });


            //新增修改ModelLineHR按钮
            $("#js_btn_save_edit_ModelLineHR").click('click', function () {
                var editform = $('#js_form_ModelLineHR_edit');
                if (editform.valid()) {
                    var options = {
                        url: ModelLineHRMaintenance.urls.EditModelLineHR,
                        type: 'post',
                        success: function (data) {
                            if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("修改成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });

            //新增保存ModelLineHR按钮
            $("#js_btn_save_new_ModelLineHR").click('click', function () {
                var editform = $('#js_form_ModelLineHR_edit');

                if (editform.valid()) {
                    var options = {
                        url: ModelLineHRMaintenance.urls.AddModelLineHR,
                        type: 'post',
                        success: function (data) {
                            if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("新增成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });
            //#region 模板下载
            $('#js_btn_download_fl').on('click', function () {

                this.form.action = ModelLineHRMaintenance.urls.downloadExcel;
                $('#js_form_excel_add').submit();
            });
            //#endregion
            // Excel导入所有事件
            //提交     stat
            $('#btn_excel_upload').on('click', function () {
                $('#js_form_excel_upload').ajaxSubmit({
                    async: true,
                    beforeSubmit: function () {

                        var fileName = $('#js_s_input_import').val();
                        var fileExtension = fileName.substring(fileName.lastIndexOf('.') + 1);
                        if (fileExtension.toLowerCase() != "xlsx" && fileExtension.toLowerCase() != "xls") {
                            PDMS.Utility.MessageBox.info('请选择Excel文件');
                            $.unblockUI();
                            return false;
                        }
                        if (!$('#js_form_excel_upload').valid()) {
                            $.unblockUI();
                            return false;
                        }
                        $.blockUI({ message: "<h1>@T("QA.Imported")</h1>" });
                    },
                    success: function (data) {
                        $.unblockUI();
                        if (data != '') {
                            PDMS.Utility.MessageBox.error(data);
                        }
                        else {
                            $('#js_importExcel_modal').modal('hide');
                            PDMS.Utility.MessageBox.info('@T("QA.Importsuccessful")', function () {
                                window.location.reload();
                            });
                        }
                    }
                });
            });
            //提交   END
            //隐藏上传窗口时清空值-----------START
            $('#js_importExcel_modal').on('hidden.bs.modal', function (e) {
                $('#js_importExcel_modal').find('input').val('');
            });
            //隐藏上传窗口时清空值-------------END


            //Form Validate
            $('#js_form_ModelLineHR_edit').validate({
                errorContainer: $('ul.validate-error'),
                errorLabelContainer: $('#js_edit_modal ul.validate-error'),
                wrapper: 'li'
            });
        });
    </script>
