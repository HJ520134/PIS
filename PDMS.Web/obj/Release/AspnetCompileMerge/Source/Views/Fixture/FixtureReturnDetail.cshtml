@model IEnumerable<PDMS.Model.EntityDTO.Fixture_Return_DDTO>
@{
    var entities = ViewBag.Entity;
}
<section class="content-header portal-content-header">
    <h1>
        治具领用详细
    </h1>
</section>
<section class="content portal-content">

    <div class="row">
        <div class="col-xs-12" id="js_criteria_alert_cnt">
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="col-md-12 col-lg-8">
                <h4 class="margin-td-5" id="js_search_keywords"></h4>
            </div>
            <div class="col-md-12 search-field col-lg-4">
                @using (Html.BeginForm("FixtureReturnIndex", "Fixture", FormMethod.Post, new { id = "js_form_excel_download" }))
                {
                    <button type="button" class="fa  btn btn-primary" onclick="javascript:history.go(-1);">
                        <i class="glyphicon glyphicon-backward"></i>
                        返回
                    </button>
                }
            </div>
            <br />
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 table-container">
            <table class="table table-striped table-hover table-condensed nowrap" id="js_Bom_datatable">
                <thead>
                    <tr>
                        <th class="table-col-seq nosort">序号</th>
                        <th>是否归还</th>
                        <th>厂别</th>
                        <th>BG</th>
                        <th>功能厂</th>
                        <th>制程</th>
                        <th>工站</th>
                        <th>线别</th>
                        <th>设备编号</th>
                        <th>治具唯一编号</th>
                        <th>治具短码</th>
                        <th>创建者</th>
                        <th>创建日期</th>
                        <th>修改者</th>
                        <th>修改日期</th>
                    </tr>
                </thead>
                <tbody>
                    @{ 
                        int i = 0;

                        foreach (var item in entities)
                        {
                            i = i + 1;
                        <tr>
                            <td>@i</td>
                            @if (item.IS_Return == 1)
                            {
                                <td><input type="checkbox" checked disabled/></td>
                            }
                            else
                            {
                                <td><input type="checkbox" disabled /></td>
                            }
                            <td>@item.Plant</td>
                            <td>@item.BG</td>
                            <td>@item.FunPlant</td>
                            <td>@item.Process_Name</td>
                            <td>@item.WorkStation_Name</td>
                            <td>@item.Line_NO</td>
                            <td>@item.Machine_ID</td>
                            <td>@item.Fixture_Unique_ID</td>
                            <td>@item.ShortCode</td>
                            <td>@item.User_Name</td>
                            <td>@item.Created_Date</td>
                            <td>@item.ModifyUserName</td>
                            <td>@item.Modified_Date</td>
                        </tr>
                    }
                    }
                </tbody>
                <tfoot>
                    <tr>
                        <th class="table-col-seq nosort">序号</th>
                        <th>是否归还</th>
                        <th>厂别</th>
                        <th>BG</th>
                        <th>功能厂</th>
                        <th>制程</th>
                        <th>工站</th>
                        <th>线别</th>
                        <th>设备编号</th>
                        <th>治具唯一编号</th>
                        <th>治具短码</th>
                        <th>创建者</th>
                        <th>创建日期</th>
                        <th>修改者</th>
                        <th>修改日期</th>
                    </tr>
                </tfoot>
            </table>
            <div id="page" class="row"></div>

        </div><!--/表格-->
    </div>

</section>

@section ViewScripts{
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-select.css" />
    <script type="text/javascript" src="~/Scripts/EQP/bootstrap-select.js"></script>
}
