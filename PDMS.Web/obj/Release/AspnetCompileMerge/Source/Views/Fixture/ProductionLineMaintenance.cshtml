                        }
                    });
                }
            }

            //OP类型变更  start
            $('#js_select_optype_query').change(function () {
                $('#js_select_funplant_query').empty();
                $('#js_select_funplant_query').html('<option></option>');

                $('#js_select_project_query').empty();
                $('#js_select_project_query').html('<option></option>');

                $('#js_select_Workshop_query').empty();
                $('#js_select_Workshop_query').html('<option></option>');

                $('#js_select_Workstation_query').empty();
                $('#js_select_Workstation_query').html('<option></option>');

                $('#js_select_project_query').empty();
                $('#js_select_project_query').html('<option></option>');

                $('#js_select_process_query').empty();
                $('#js_select_process_query').html('<option></option>');
                if ($('#js_select_optype_query option:selected').text() != "") {

                    var factoryVal = $('#js_select_factory_query').val();
                    var optypeVal = $('#js_select_optype_query').val()
                    var funplantVal = $('#js_select_funplant_query').val();
                    if (funplantVal == "" || funplantVal == undefined || funplantVal == false) {
                        funplantVal = 0;
                    }
                    var url = Production_LineMaintenance.urls.getFunPlantByOPTypes;
                    $.post(url, { Optype: optypeVal }, function (data) {
                        for (var i = 0; i < data.length; i++) {
                            $('#js_select_funplant_query').append('<option value="' + data[i].FunPlant_OrganizationUID + '">' + data[i].FunPlant + '</option>');
                        }
                    });

                    if (optypeVal > 0) {
                        var getProjectByOPTypeUrl = Production_LineMaintenance.urls.getProjectByOPType;
                        $.get(getProjectByOPTypeUrl, { oporgid: optypeVal }, function (data) {
                            for (var i = 0; i < data.length; i++) {
                                $('#js_select_project_query').append('<option value="' + data[i].Project_UID + '">' + data[i].Project_Code + "_" + data[i].Project_Name + '</option>');
                            }
                        });

                        var getWorkshopUrl = Production_LineMaintenance.urls.getWorkshopList;
                        $.post(getWorkshopUrl, { Plant_Organization_UID: factoryVal, BG_Organization_UID: optypeVal, FunPlant_Organization_UID: funplantVal }, function (data) {
                            for (var i = 0; i < data.length; i++) {
                                $('#js_select_Workshop_query').append('<option value="' + data[i].Workshop_UID + '">' + data[i].Workshop_ID + "_" + data[i].Workshop_Name + '</option>');
                            }
                        });

                        var getWorkstationUrl = Production_LineMaintenance.urls.getWorkstationList;
                        $.post(getWorkstationUrl, { Plant_Organization_UID: factoryVal, BG_Organization_UID: optypeVal, FunPlant_Organization_UID: funplantVal }, function (data) {
                            for (var i = 0; i < data.length; i++) {
                                $('#js_select_Workstation_query').append('<option value="' + data[i].WorkStation_UID + '">' + data[i].WorkStation_ID + "_" + data[i].WorkStation_Name + '</option>');
                            }
                        });

                        var getProcessUrl = Production_LineMaintenance.urls.getProcess_InfoList;
                        $.post(getProcessUrl, { Plant_Organization_UID: factoryVal, BG_Organization_UID: optypeVal, FunPlant_Organization_UID: funplantVal }, function (data) {
                            for (var i = 0; i < data.length; i++) {
                                $('#js_select_process_query').append('<option value="' + data[i].Process_Info_UID + '">' + data[i].Process_ID + "_" + data[i].Process_Name + '</option>');
                            }
                        });
                    }
                }
            })

            //功能厂change
            $('#js_select_funplant_query').change(function () {
                url = Production_LineMaintenance.urls.getProcess_InfoList;
                $('#js_select_process_query').empty();
                $('#js_select_process_query').html('<option></option>');
                $.post(url, { Plant_Organization_UID: $('#js_select_factory_query').val(), BG_Organization_UID: $('#js_select_optype_query').val(), FunPlant_Organization_UID: $('#js_select_funplant_query').val() }, function (data) {
                    for (var i = 0; i < data.length; i++) {
                        $('#js_select_process_query').append('<option value="' + data[i].Process_Info_UID + '">' + data[i].Process_ID + "_" + data[i].Process_Name + '</option>');
                    }
                });
            });


            $('#js_select_funplant_add').change(function () {
                refreshWorkshopSelectAdd();
                refreshWorkStationSelectAdd();
                refreshProjectSelectAdd();
                refreshProcessSelectAdd();
            });


            //新增修改Production_Line按钮
            $("#js_btn_save_edit_Production_Line").click('click', function () {
                var editform = $('#js_form_Production_Line_edit');
                if (editform.valid()) {
                    var options = {
                        url: Production_LineMaintenance.urls.EditProduction_Line,
                        type: 'post',
                        success: function (data) {
                            if (data == 'EXIST') {
                                PDMS.Utility.MessageBox.error("修改生产线[厂区][OP类型][生产线代码][生产线编码]重复");
                            }
                            else if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("修改生产线信息成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });

            //新增保存Production_Line按钮
            $("#js_btn_save_new_Production_Line").click('click', function () {
                $('#js_hidden_Plant_Organization_uid').val($('#js_select_factory_add option:selected').val());
                $('#js_hidden_BG_Organization_uid').val($('#js_select_optype_add option:selected').val());
                $('#js_hidden_FunPlant_Organization_uid').val($('#js_select_funplant_add option:selected').val());

                var editform = $('#js_form_Production_Line_edit');
                if (editform.valid()) {
                    var options = {
                        url: Production_LineMaintenance.urls.AddProduction_Line,
                        type: 'post',
                        success: function (data) {
                            if (data == 'EXIST') {
                                PDMS.Utility.MessageBox.error("新增生产线[厂区][OP类型][生产线代码][生产线编码]重复");
                            }
                            else if (data == 'SUCCESS') {
                                PDMS.Utility.MessageBox.info("新增生产线信息成功", function () {
                                    window.location.reload();
                                });
                            } else {
                                PDMS.Utility.MessageBox.error(data);
                            }
                        }
                    };
                    editform.ajaxSubmit(options);
                }
            });
            //#region 模板下载
            $('#js_btn_download_fl').on('click', function () {
                
                this.form.action = Production_LineMaintenance.urls.downloadExcel;
                $('#js_form_excel_add').submit();
            });
            //#endregion
            //提交     stat
            $('#btn_excel_upload').on('click', function () {
                $('#js_form_excel_upload').ajaxSubmit({
                    async: true,
                    beforeSubmit: function () {

                        var fileName = $('#js_s_input_import').val();
                        var fileExtension = fileName.substring(fileName.lastIndexOf('.') + 1);
                        if (fileExtension.toLowerCase() != "xlsx" && fileExtension.toLowerCase() != "xls") {
                            PDMS.Utility.MessageBox.info('请选择Excel文件');
                            $.unblockUI();
                            return false;
                        }
                        if (!$('#js_form_excel_upload').valid()) {
                            $.unblockUI();
                            return false;
                        }
                        $.blockUI({ message: "<h1>@T("QA.Imported")</h1>" });
                    },
                    success: function (data) {
                        $.unblockUI();
                        if (data != '') {
                            PDMS.Utility.MessageBox.error(data);
                        }
                        else {
                            $('#js_importExcel_modal').modal('hide');
                            PDMS.Utility.MessageBox.info('@T("QA.Importsuccessful")', function () {
                                window.location.reload();
                            });
                        }
                    }
                });
            });
            //提交   END
            //隐藏上传窗口时清空值-----------START
            $('#js_importExcel_modal').on('hidden.bs.modal', function (e) {
                $('#js_importExcel_modal').find('input').val('');
            });
            //隐藏上传窗口时清空值-------------END



            //Form Validate
            $('#js_form_Production_Line_edit').validate({
                errorContainer: $('ul.validate-error'),
                errorLabelContainer: $('#js_edit_modal ul.validate-error'),
                wrapper: 'li'
            });

        });

        function changeWorkStation(sender) {
            
            var selectedWorkStation = $(sender).find("option:selected");
            var projectUID = selectedWorkStation.attr("data-Project_UID");
            var projectName = selectedWorkStation.attr("data-Project_Name");
            var processUID = selectedWorkStation.attr("data-Process_Info_UID");
            var processName = selectedWorkStation.attr("data-Process_Name");

            $("#js_select_project_add").val(projectUID);
            $("#js_select_projectName_add").val(projectName);
            $("#js_select_process_add").val(processUID);
            $("#js_select_processName_add").val(processName);
        }
    </script>
}