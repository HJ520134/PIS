                    }
                    else {
                        return null;
                    }
                };

                var _queryProduction_Lines = function (firstLoad) {

                    var config = {
                        pageId: "#page",
                        tableId: "#js_Production_Line_datatable",
                        remoteUrl: urls.queryProduction_Lines,
                        searchParams: _getParams(),
                        tableOptions: {
                            scrollX: true,
                            scrollCollapse: false,
                            autoWidth: true,
                            columns: columns
                        }
                    };

                    if (!firstLoad) {
                        $('#page').page('destroy');
                    }

                    if (needBuildCriteria) {
                        PDMS.Utility.Criteria.Build();
                    }

                    PDMS.Utility.Pages.Set(config);
                };

                return {
                    urls: urls,
                    Init: function () {
                        //页面初始化加载查询时，初始化Criteria
                        PDMS.Utility.Criteria.Init();
                        _queryProduction_Lines(true);
